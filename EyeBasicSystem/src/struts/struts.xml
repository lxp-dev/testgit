<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
    
<struts>
	<constant name="struts.custom.i18n.resources" value="messages"/>
    <!-- 配置常量  -->
    <constant name="struts.enable.DynamicMethodInvocation" value="false" />
    <!-- 该属性设置浏览器是否缓存静态内容 -->
    <constant name="struts.serve.static.browserCache" value="true"></constant>
	
	<constant name="struts.multipart.maxSize" value="100000000" />
	<constant name="struts.multipart.saveDir" value="D:/temp" />    
    <!-- 配置开发模式为false -->
    <constant name="struts.devMode" value="false" />
    
    <include file="components.xml" />
    <include file="login.xml" />
	<include file="system.xml" />    
	<include file="basic.xml" />
	<include file="storage.xml" />
	<include file="member.xml" />
	<include file="sales.xml" />
	<include file="casehistory.xml" />
	<include file="hydsycasehistory.xml" />		
	<include file="module.xml" />
	<include file="report.xml" />
	<include file="logistics.xml" />
	<include file="plugin.xml" />
	<include file="personnel.xml" />
	<include file="weixin.xml" />
	<include file="mall.xml" />
	<include file="yklogistics.xml" />
	<include file="bjtrhistory.xml" />
	<include file="aierhistory.xml" />	
    <include file="his.xml" />
		
</struts>