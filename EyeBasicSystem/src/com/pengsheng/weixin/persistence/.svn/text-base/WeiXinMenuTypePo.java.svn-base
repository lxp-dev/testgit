package com.pengsheng.weixin.persistence;

public class WeiXinMenuTypePo {
	private String wmtid;
	private String wmtname;
	private String wmtbuttontype;
	private String wmtdescription;
	
	public String getWmtid() {
		return wmtid;
	}
	public void setWmtid(String wmtid) {
		this.wmtid = wmtid;
	}
	public String getWmtname() {
		return wmtname;
	}
	public void setWmtname(String wmtname) {
		this.wmtname = wmtname;
	}
	public String getWmtbuttontype() {
		return wmtbuttontype;
	}
	public void setWmtbuttontype(String wmtbuttontype) {
		this.wmtbuttontype = wmtbuttontype;
	}
	public String getWmtdescription() {
		return wmtdescription;
	}
	public void setWmtdescription(String wmtdescription) {
		this.wmtdescription = wmtdescription;
	}
}