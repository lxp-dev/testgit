package com.pengsheng.eims.personnel.persistence;

public class WorkagePo {

	private String mwpid; // 主键
	private String mwpname; // 工龄名称
	private String mwpsalary; // 对应津贴

	public String getMwpid() {
		return mwpid;
	}

	public void setMwpid(String mwpid) {
		this.mwpid = mwpid;
	}

	public String getMwpname() {
		return mwpname;
	}

	public void setMwpname(String mwpname) {
		this.mwpname = mwpname;
	}

	public String getMwpsalary() {
		return mwpsalary;
	}

	public void setMwpsalary(String mwpsalary) {
		this.mwpsalary = mwpsalary;
	}
}
