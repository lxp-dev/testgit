package com.pengsheng.eims.logistics.persistence;

public class ReversalPo {
	
	private String lirid;// 发票号
	private String lirsupplierid;// 制造商ID
	private String lirdate;// 发票日期
	private String lirdepartmentid;// 部门ID
	private String lircreatepersonid;// 核销人ID
	private String lirauditstatue;// 审核状态 0:未审核 1:已审核
	private String lirauditpersonid;// 审核人ID
	private String lirauditdate;// 审核日期
	private String lirisvoucher;// 凭证标识 0:非凭证 1:凭证
	private String lirvoucherdate;// 凭证日期
	private String lirremark;// 备注
	
	public String getLirid() {
		return lirid;
	}
	public void setLirid(String lirid) {
		this.lirid = lirid;
	}
	public String getLirsupplierid() {
		return lirsupplierid;
	}
	public void setLirsupplierid(String lirsupplierid) {
		this.lirsupplierid = lirsupplierid;
	}
	public String getLirdate() {
		return lirdate;
	}
	public void setLirdate(String lirdate) {
		this.lirdate = lirdate;
	}
	public String getLirdepartmentid() {
		return lirdepartmentid;
	}
	public void setLirdepartmentid(String lirdepartmentid) {
		this.lirdepartmentid = lirdepartmentid;
	}
	public String getLircreatepersonid() {
		return lircreatepersonid;
	}
	public void setLircreatepersonid(String lircreatepersonid) {
		this.lircreatepersonid = lircreatepersonid;
	}
	public String getLirauditstatue() {
		return lirauditstatue;
	}
	public void setLirauditstatue(String lirauditstatue) {
		this.lirauditstatue = lirauditstatue;
	}
	public String getLirauditpersonid() {
		return lirauditpersonid;
	}
	public void setLirauditpersonid(String lirauditpersonid) {
		this.lirauditpersonid = lirauditpersonid;
	}
	public String getLirauditdate() {
		return lirauditdate;
	}
	public void setLirauditdate(String lirauditdate) {
		this.lirauditdate = lirauditdate;
	}
	public String getLirisvoucher() {
		return lirisvoucher;
	}
	public void setLirisvoucher(String lirisvoucher) {
		this.lirisvoucher = lirisvoucher;
	}
	public String getLirvoucherdate() {
		return lirvoucherdate;
	}
	public void setLirvoucherdate(String lirvoucherdate) {
		this.lirvoucherdate = lirvoucherdate;
	}
	public String getLirremark() {
		return lirremark;
	}
	public void setLirremark(String lirremark) {
		this.lirremark = lirremark;
	}

}
