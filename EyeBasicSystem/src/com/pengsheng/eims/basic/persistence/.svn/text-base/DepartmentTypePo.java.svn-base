package com.pengsheng.eims.basic.persistence;

public class DepartmentTypePo {
	
	//部门类型id
	private String bdtid;	
	//部门类型名称
	private String bdtname;	
	//部门类型      1.销售门店、2.加工中心、3.总仓库房、4.财务、5.其他、6.人事部门
	public String getBdtid() {
		return bdtid;
	}
	public void setBdtid(String bdtid) {
		this.bdtid = bdtid;
	}
	public String getBdtname() {
		return bdtname;
	}
	public void setBdtname(String bdtname) {
		this.bdtname = bdtname;
	}
}
