package com.pengsheng.eims.basic.persistence;

public class ColorPo {

	private String bcid;
	private String bccolorname;
	
	//镜架商品款式，与颜色PO合并使用
	private String bfsid;
	private String bfsname;
	
	public String getBfsid() {
		return bfsid;
	}
	public void setBfsid(String bfsid) {
		this.bfsid = bfsid;
	}
	public String getBfsname() {
		return bfsname;
	}
	public void setBfsname(String bfsname) {
		this.bfsname = bfsname;
	}
	public String getBcid() {
		return bcid;
	}
	public void setBcid(String bcid) {
		this.bcid = bcid;
	}
	public String getBccolorname() {
		return bccolorname;
	}
	public void setBccolorname(String bccolorname) {
		this.bccolorname = bccolorname;
	}

}
