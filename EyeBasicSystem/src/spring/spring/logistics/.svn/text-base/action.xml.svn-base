<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName" default-lazy-init="false" default-dependency-check="none">

	<!-- 发票核销   begin-->
		<bean id="invoiceAction" class="com.pengsheng.eims.logistics.action.InvoiceAction" scope="prototype"/>
	<!-- 发票核销   end-->


	<!-- 发票单据选择开窗  wanglei begin-->
		<bean id="invoiceSelectBillAction" class="com.pengsheng.eims.logistics.action.InvoiceSelectBillAction" scope="prototype"/>
	<!-- 发票单据选择开窗  wanglei end-->
	
	<!-- 凭证单据选择开窗  wanglei begin-->
		<bean id="proofSelectBillAction" class="com.pengsheng.eims.logistics.action.ProofSelectBillAction" scope="prototype"/>
	<!-- 凭证单据选择开窗  wanglei end-->
	

	<!-- 凭证管理  SZK begin-->
	    <bean id="voucherAction" class="com.pengsheng.eims.logistics.action.VoucherAction" scope="prototype"/>
	    <bean id="voucherTallyAction" class="com.pengsheng.eims.logistics.action.VoucherTallyAction" scope="prototype"/>
	    <bean id="outStrogeVoucherAction" class="com.pengsheng.eims.logistics.action.OutStrogeVoucherAction" scope="prototype"/>
	<!-- 凭证管理  SZK end-->

	<!-- 凭证单据选择开窗  wanglei begin-->
		<bean id="calAction" class="com.pengsheng.eims.logistics.action.CalAction" scope="prototype"/>
	<!-- 凭证单据选择开窗  wanglei end-->
	
    <!-- 物流日志   SZK begin -->
    <bean id="logisticsLogAction" class="com.pengsheng.eims.logistics.action.LogisticsLogAction" scope="prototype"/>    
    <!-- 物流日志   SZK end -->

    <!-- 付款单   SZK begin -->
    <bean id="payMentBillAction" class="com.pengsheng.eims.logistics.action.PayMentBillAction" scope="prototype"/>    
    <!-- 付款单   SZK end -->

    <!-- 凭证模板   SZK begin -->
    <bean id="voucherTemplateAction" class="com.pengsheng.eims.logistics.action.VoucherTemplateAction" scope="prototype"/>    
    <!-- 凭证模板   SZK end -->
    
    <!-- 收款单   SZK begin -->
    <bean id="receiptMentBillAction" class="com.pengsheng.eims.logistics.action.ReceiptMentBillAction" scope="prototype"/>    
    <!-- 收款单   SZK end -->
    
</beans>