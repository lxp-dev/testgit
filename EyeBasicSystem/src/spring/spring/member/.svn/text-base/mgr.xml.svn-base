<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-autowire="byName" default-lazy-init="false"
	default-dependency-check="none">
	<!-- 会员人群分类维护 moyongsheng  begin-->
	<bean id="crowdCategoriesMgrImpl"
		class="com.pengsheng.eims.member.mgr.impl.CrowdCategoriesMgrImpl" />
	<bean id="crowdCategoriesMgr" parent="baseTransactionProxy">
		<property name="target" ref="crowdCategoriesMgrImpl" />
	</bean>
	<!-- 会员人群分类维护 moyongsheng  end-->	
	
	<!-- 顾客满意度表 moyongsheng  begin-->
	<bean id="customerSatisfactionMgrImpl"
		class="com.pengsheng.eims.member.mgr.impl.CustomerSatisfactionMgrImpl" />
	<bean id="customerSatisfactionMgr" parent="baseTransactionProxy">
		<property name="target" ref="customerSatisfactionMgrImpl" />
	</bean>
	<!-- 顾客满意度表 moyongsheng  end-->

	<!-- 顾客基本资料 moyongsheng  begin-->
	<bean id="customerInfoMgrImpl"
		class="com.pengsheng.eims.member.mgr.impl.CustomerInfoMgrImpl" />
	<bean id="customerInfoMgr" parent="baseTransactionProxy">
		<property name="target" ref="customerInfoMgrImpl" />
	</bean>
	<!-- 顾客基本资料 moyongsheng  end-->	
	
	<!-- 会员卡升级 wanglei  begin-->
	<bean id="memberUpGradeMgrImpl"
		class="com.pengsheng.eims.member.mgr.impl.MemberUpGradeMgrImpl" />
	<bean id="memberUpGradeMgr" parent="baseTransactionProxy">
		<property name="target" ref="memberUpGradeMgrImpl" />
	</bean>
	<!-- 会员卡升级 wanglei  end-->	
	
	<!-- 顾客回访 wanglei  begin-->
	<bean id="customerReturnVisitMgrImpl"
		class="com.pengsheng.eims.member.mgr.impl.CustomerReturnVisitMgrImpl" />
	<bean id="customerReturnVisitMgr" parent="baseTransactionProxy">
		<property name="target" ref="customerReturnVisitMgrImpl" />
	</bean>
	<!-- 顾客回访 wanglei  end-->	
	
	<!-- 短息维护 qiyue  begin-->
	<bean id="smsSetMgrImpl"
		class="com.pengsheng.eims.member.mgr.impl.SmsSetMgrImpl" />
	<bean id="smsSetMgr" parent="baseTransactionProxy">
		<property name="target" ref="smsSetMgrImpl" />
	</bean>
	<!--  短息维护 qiyue  end-->	
</beans>