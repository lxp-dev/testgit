<HTML><HEAD><TITLE>Page Name</TITLE>
<META http-equiv=Content-Type content="text/html; charset=utf-8">
<META http-equiv=pragma content=no-cache>
<META http-equiv=cache-control content=no-cache>
<META http-equiv=expires content=0>

<SCRIPT language=javascript type=text/javascript>

function toTop(){
	changeSizeUp();
}
function toDown(){
	changeSizeDown();
}
var pic=new Array("img/frame/v_close.jpg","img/frame/v_close.jpg","img/frame/v_open.jpg","img/frame/v_open.jpg");
	
	function changeSize(){
		var leftorright=document.getElementById("toptag").value;
		var frame = top.document.getElementById("total");
		frame.rows=(frame.rows=="99,8,*,15")?"0,8,*,15":"99,8,*,15";
		if (frame.rows=="0,8,*,15"){
				document.getElementById('a1').innerHTML="<img src=\"img/frame/v_open.jpg\" onmouseover=\"this.src=pic[3]\" onmouseout=\"this.src=pic[2]\" onclick=\"changeSize()\" style=\"cursor:hand\" alt=\"显示上栏\">";
				document.getElementById('toptag').value='down';
		}
		else{
				document.getElementById('a1').innerHTML="<img src=\"img/frame/v_close.jpg\" onmouseover=\"this.src=pic[1]\" onmouseout=\"this.src=pic[0]\" onclick=\"changeSize()\" style=\"cursor:hand\" alt=\"隐藏上栏\">";
				document.getElementById('toptag').value='up';			
			}
	}
		
		function changeSizeUp(){
			var frame = top.document.getElementById("total");
			frame.rows="0,8,*,15";
			
			document.getElementById('a1').innerHTML="<img src=\"img/frame/v_open.jpg\" onmouseover=\"this.src=pic[3]\" onmouseout=\"this.src=pic[2]\" onclick=\"changeSize()\" style=\"cursor:hand\" alt=\"显示上栏\">";
			leftorright='down';
			
		}
		
		function changeSizeDown(){
			var frame = top.document.getElementById("total");
			frame.rows= "99,8,*,15";
			
			document.getElementById('a1').innerHTML="<img src=\"img/frame/v_close.jpg\" onmouseover=\"this.src=pic[1]\" onmouseout=\"this.src=pic[0]\" onclick=\"changeSize()\" style=\"cursor:hand\" alt=\"隐藏上栏\">";
			leftorright='up';
			
		}
</SCRIPT>

<META content="MSHTML 6.00.2900.5848" name=GENERATOR></HEAD>
<BODY bgColor=#e2edf5 leftMargin=0 topMargin=0 scroll=no>
<input type="hidden" id="toptag" value="up">
<TABLE cellSpacing=0 cellPadding=0 width="100%" background="" border=0>
  <TR>
<TD id=a1 align=center height=8><IMG src="img/frame/v_close.jpg" alt=隐藏上栏 border="0" style="CURSOR: hand" onclick=changeSize() ></TD></TR></TABLE></BODY>
</HTML>
