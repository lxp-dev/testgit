<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select departmentID into #shopCode from sales_createShopCodeTab('${departmentsID}')
select salesArea into #salesarea from sales_createSalesAreaTab('${priceArea}','${reportID}')

select areaID as areaID,shopName as shopName,isnull(sum(salesNum),0) as salesNum,isnull(sum(salesAmount),0) as salesAmount from (
select R_SD_DCAE_AreaID as areaID,R_SD_DCAE_ShopName as shopName,isnull(sum(R_SD_DCAE_SalesNum),0) as salesNum,
       isnull(sum(R_SD_DCAE_SalesAmount),0) as salesAmount 
   from R_SD_DayCollectAreaEntry
   where R_SD_DCAE_SalesType in ('1','2')
   and ((isnull('${bgnDate}','')<>'' and convert(varchar(10),R_SD_DCAE_Date,120)>='${bgnDate}') or (isnull('${bgnDate}','')=''))
   and ((isnull('${endDate}','')<>'' and convert(varchar(10),R_SD_DCAE_Date,120)<='${endDate}') or (isnull('${endDate}','')=''))
   and ((isnull('${departmentsID}','')<>'' and R_SD_DCAE_ShopCode in (select departmentID from #shopCode )) or (isnull('${departmentsID}','')=''))
   and ((isnull('${priceArea}','')<>'' and R_SD_DCAE_AreaID in (select salesarea from #salesarea )) or (isnull('${priceArea}','')=''))
   and ((isnull('${salesType}','')<>'' and R_SD_DCAE_SalesType='${salesType}') or (isnull('${salesType}','')=''))
   group by R_SD_DCAE_AreaID,R_SD_DCAE_ShopName
union all
select R_SD_DCAE_AreaID as areaID,R_SD_DCAE_ShopName as shopName,isnull(sum(R_SD_DCAE_GlassesFrameNum),0) as salesNum,
       isnull(sum(R_SD_DCAE_GlassesFrameRateAmount),0) as salesAmount 
   from R_SD_DayCollectAreaEntry
   where R_SD_DCAE_SalesType='5'
   and ((isnull('${bgnDate}','')<>'' and convert(varchar(10),R_SD_DCAE_Date,120)>='${bgnDate}') or (isnull('${bgnDate}','')=''))
   and ((isnull('${endDate}','')<>'' and convert(varchar(10),R_SD_DCAE_Date,120)<='${endDate}') or (isnull('${endDate}','')=''))
   and ((isnull('${departmentsID}','')<>'' and R_SD_DCAE_ShopCode in (select departmentID from #shopCode )) or (isnull('${departmentsID}','')=''))
   and ((isnull('${priceArea}','')<>'' and R_SD_DCAE_AreaID in (select salesarea from #salesarea )) or (isnull('${priceArea}','')=''))
   and ((isnull('${salesType}','')<>'' and R_SD_DCAE_SalesType='${salesType}') or (isnull('${salesType}','')=''))
   group by R_SD_DCAE_AreaID,R_SD_DCAE_ShopName
)temp
   group by areaID,shopName
   order by cast(substring(areaID,1,CHARINDEX('-',areaID)-1) as float)

drop table #shopCode
drop table #salesarea]]></Query>
</TableData>
