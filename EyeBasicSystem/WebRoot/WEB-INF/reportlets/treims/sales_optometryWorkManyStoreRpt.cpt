                               AND S_OP_OY_ShopCode  in (select departmentID from #salesDepartments)
                        GROUP  BY i1.S_OP_OY_ShopCode)pr
               ON p.PersonID = pr.S_OP_OY_ShopCode
             LEFT JOIN (SELECT i2.S_OP_OY_ShopCode
                               --,isnull(sum(sp.sNum),0) as sNum
