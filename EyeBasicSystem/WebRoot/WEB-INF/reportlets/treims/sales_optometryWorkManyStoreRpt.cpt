                                 ON i2.S_OP_OY_OptometryID = tsp.S_SE_SB_OptometryID
                        GROUP  BY i2.S_OP_OY_ShopCode)s
               ON p.PersonID = s.S_OP_OY_ShopCode
             LEFT JOIN (SELECT i3.S_OP_OY_ShopCode,
                               SUM(opp.prNum) AS prNum --打印处方数
