<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select typeID into #salesTypes from sales_createSalesType('${salestype}') 
select typeID into #categorys from sales_createSalesType('${goodsCategoryID}')

select ROW_NUMBER() Over(order by bgcid,supplierID) as rowNum,B_BD_ID,B_BD_brandName,bgcid,GoodsCategoryName,bgnDate+' -- '+endDate as printdate,departmentName,supplierID,supplierName,
       goodsQuantity,notTaxRateAmount,taxAmount,costPriceAmount,taxRate from (

select B_BD_ID,B_BD_brandName,B_SP_ID as supplierID,B_GC_ID as bgcid,B_GC_GoodsCategoryName as GoodsCategoryName,'${bgnDate}' as bgnDate,'${endDate}' as endDate,
       (select top 1 B_DP_DepartmentName from B_Departments where B_DP_DepartmentID='${departmentID}') as departmentName,
       B_SP_SupplierName as supplierName,sum(C_ST_IE_GoodsQuantity) as goodsQuantity,
       sum(isnull(C_ST_IE_NotTaxRateAmount,0)) as notTaxRateAmount,sum(isnull(C_ST_IE_TaxAmount,0)) as taxAmount,
       sum(isnull(C_ST_IE_CostPriceAmount,0)) as costPriceAmount,C_ST_IE_TaxRate as taxRate
  from C_ST_Inventory 
	   inner join C_ST_InventoryEntry on C_ST_I_BillID=C_ST_IE_BillID
       left join B_Supplier on substring(C_ST_IE_GoodsId,3,2)=B_SP_ID
	   left join B_Brand on substring(C_ST_IE_GoodsId,6,2) = B_BD_ID and substring(C_ST_IE_GoodsId,3,2) = B_BD_SupplierID
	   left join B_GoodsCategory on substring(C_ST_IE_GoodsId,1,1)=B_GC_ID 
	   left join B_GoodsInfo on  B_GoodsInfo.B_GI_GoodsID=C_ST_InventoryEntry.C_ST_IE_GoodsId
       left join SYS_PersonInfo a on C_ST_I_createPerson=a.id
       left join SYS_PersonInfo b on C_ST_I_FinanceAuditPerson=b.id
  where C_ST_I_BillTypeId in ('1','9') and C_ST_I_AuditState = '1' 
   and ((isnull('${departmentID}','')<>'' and C_ST_I_InStockId in (select B_WH_ID from B_Warehouse where B_WH_deptID='${departmentID}')) or (isnull('${departmentID}','')=''))
   and convert(varchar(10),C_ST_I_AuditDate,120)>='${bgnDate}'
   and convert(varchar(10),C_ST_I_AuditDate,120)<='${endDate}'
   and ((isnull('${supplierid}','') <> '' and B_SP_ID='${supplierid}') or (isnull('${supplierid}','')=''))
   and ((isnull('${brandID}','') <> '' and substring(C_ST_IE_GoodsId,6,2)='${brandID}') or (isnull('${brandID}','')=''))
   and ( '${maxprice}' = '' or '${maxprice}' = null or C_ST_IE_NotTaxRate <= cast('${maxprice}' as float))
   and ( '${minprice}' = '' or '${minprice}' = null or C_ST_IE_NotTaxRate >= cast('${minprice}' as float)) 
and ((isnull('${salestype}','')<>'' and B_BD_Settlement in (select typeID from #salesTypes)) or isnull('${salestype}','')='') 
and ((isnull('${goodsCategoryID}','')<>'' and B_GI_GoodsCategoryID in (select typeID from #categorys)) or isnull('${goodsCategoryID}','')='') 
  group by B_BD_ID,B_BD_brandName,B_SP_ID,B_GC_ID,B_GC_GoodsCategoryName,B_SP_SupplierName,C_ST_IE_TaxRate
union all
select B_BD_ID,B_BD_brandName,B_SP_ID as supplierID,B_GC_ID as bgcid,B_GC_GoodsCategoryName as GoodsCategoryName,'${bgnDate}' as bgnDate,'${endDate}' as endDate,
       (select top 1 B_DP_DepartmentName from B_Departments where B_DP_DepartmentID='${departmentID}') as departmentName,
       B_SP_SupplierName as supplierName,-sum(C_ST_IE_GoodsQuantity) as goodsQuantity,
       -sum(isnull(C_ST_IE_NotTaxRateAmount,0)) as notTaxRateAmount,-sum(isnull(C_ST_IE_TaxAmount,0)) as taxAmount,
       -sum(isnull(C_ST_IE_CostPriceAmount,0)) as costPriceAmount,C_ST_IE_TaxRate as taxRate
  from C_ST_Inventory 
	   inner join C_ST_InventoryEntry on C_ST_I_BillID=C_ST_IE_BillID
       left join B_Supplier on substring(C_ST_IE_GoodsId,3,2)=B_SP_ID
	   left join B_Brand on substring(C_ST_IE_GoodsId,6,2) = B_BD_ID and substring(C_ST_IE_GoodsId,3,2) = B_BD_SupplierID
	   left join B_GoodsCategory on substring(C_ST_IE_GoodsId,1,1)=B_GC_ID
	   left join B_GoodsInfo on  B_GoodsInfo.B_GI_GoodsID=C_ST_InventoryEntry.C_ST_IE_GoodsId
       left join SYS_PersonInfo a on C_ST_I_createPerson=a.id
       left join SYS_PersonInfo b on C_ST_I_FinanceAuditPerson=b.id
  where C_ST_I_BillTypeId='2' and C_ST_I_AuditState = '1' 
   and ((isnull('${departmentID}','')<>'' and C_ST_I_OutStockId in (select B_WH_ID from B_Warehouse where B_WH_deptID='${departmentID}')) or (isnull('${departmentID}','')=''))
   and convert(varchar(10),C_ST_I_AuditDate,120)>='${bgnDate}'
   and convert(varchar(10),C_ST_I_AuditDate,120)<='${endDate}'
   and ((isnull('${supplierid}','') <> '' and B_SP_ID='${supplierid}') or (isnull('${supplierid}','')=''))
   and ((isnull('${brandID}','') <> '' and substring(C_ST_IE_GoodsId,6,2)='${brandID}') or (isnull('${brandID}','')=''))
   and ( '${maxprice}' = '' or '${maxprice}' = null or C_ST_IE_NotTaxRate <= cast('${maxprice}' as float))
   and ( '${minprice}' = '' or '${minprice}' = null or C_ST_IE_NotTaxRate >= cast('${minprice}' as float)) 
and ((isnull('${salestype}','')<>'' and B_BD_Settlement in (select typeID from #salesTypes)) or isnull('${salestype}','')='')  
and ((isnull('${goodsCategoryID}','')<>'' and B_GI_GoodsCategoryID in (select typeID from #categorys)) or isnull('${goodsCategoryID}','')='')  
  group by  B_BD_ID,B_BD_brandName, B_SP_ID,B_GC_ID,B_GC_GoodsCategoryName,B_SP_SupplierName,C_ST_IE_TaxRate
)temp

drop table #salesTypes
drop table #categorys]]></Query>
</TableData>
