<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select departmentID into #salesDepartments from sales_createShopCodeTab('${departmentid}')

select	'${beg}'+'--'+'${end}' as printdate,
	PosDatetime,
	cName,
	Phone,
	GoodsName,
	SupplierName,
	Batch,
	Spec,
	sum(Number) as number,
	GuaranteeperiodDatetime,
	barcode,
	RegistrationNum
from(
		select S_SE_SD_ItemID				as barcode
			   ,sum(S_SE_SD_Number)			as Number
               ,convert(varchar(10),S_SE_SB_PosDatetime,120) as PosDatetime
			   ,convert(varchar(10),S_SE_SD_Guaranteeperiod,120) as GuaranteeperiodDatetime
			   ,S_SE_SD_Batch	as Batch
			   ,S_ME_CI_Name	as cName
			   ,S_ME_CI_Phone	as Phone
			   ,B_GI_GoodsName	as GoodsName
			   ,B_GI_Spec		as Spec
			   ,B_SP_SupplierName as SupplierName
			   ,B_BD_RegistrationNum as RegistrationNum
		  from S_SE_SalesBasic 
				 left join S_SE_SalesDetail on S_SE_SB_SalesID = S_SE_SD_SalesID
				 left join B_GoodsInfo on S_SE_SD_SalesItemID = B_GI_GoodsID
				 left join B_Supplier on B_GI_SupplierID = B_SP_ID
				 left join B_Brand on B_GI_BrandID = B_BD_ID and B_GI_SupplierID = B_BD_SupplierID
				 left join S_ME_CustomerInfo on S_SE_SB_CustomerID = S_ME_CI_CustomerID
		  where S_SE_SB_ValueFlag = '1' 
				and S_SE_SB_CheckoutFlag = '0'
				and isnull(S_SE_SD_ItemID,'') <> ''
				and S_SE_SD_CommoditiesFlag in ('4','5')
				and convert(varchar(10),S_SE_SB_PosDatetime,120) >= '${beg}'
				and convert(varchar(10),S_SE_SB_PosDatetime,120) <= '${end}'
				and ((ISNULL('${cid}','')<>'' AND B_GI_GoodsCategoryID = '${cid}') OR (ISNULL('${cid}','')=''))
				and ((ISNULL('${sid}','')<>'' AND B_GI_SupplierID = '${sid}') OR (ISNULL('${sid}','')=''))
				and ((ISNULL('${bid}','')<>'' AND B_GI_BrandID = '${bid}') OR (ISNULL('${bid}','')=''))
				and ((ISNULL('${batch}','')<>'' AND S_SE_SD_Batch like '%'+'${batch}'+'%') OR (ISNULL('${batch}','')=''))
				and ((ISNULL('${departmentid}','')<>'' AND S_SE_SB_ShopCode in (select departmentID from #salesDepartments)) OR (ISNULL('${departmentid}','')=''))
		  group by	S_SE_SD_ItemID,
					convert(varchar(10),S_SE_SB_PosDatetime,120),
					convert(varchar(10),S_SE_SD_Guaranteeperiod,120),
					S_SE_SD_Batch,
					S_ME_CI_Name
					,S_ME_CI_Phone
					,B_GI_GoodsName
					,B_GI_Spec
					,B_SP_SupplierName,B_BD_RegistrationNum
		union all
		select S_SE_SD_ItemID				as barcode
			   ,-sum(S_SE_SD_Number)			as Number
               ,convert(varchar(10),S_SE_SB_PosDatetime,120) as PosDatetime
			   ,convert(varchar(10),S_SE_SD_Guaranteeperiod,120) as GuaranteeperiodDatetime
			   ,S_SE_SD_Batch	as Batch
			   ,S_ME_CI_Name	as cName
			   ,S_ME_CI_Phone	as Phone
			   ,B_GI_GoodsName	as GoodsName
			   ,B_GI_Spec		as Spec
			   ,B_SP_SupplierName as SupplierName
			   ,B_BD_RegistrationNum as RegistrationNum
		  from S_SE_SalesBasic 
				 inner join S_SE_SalesDetail on S_SE_SB_SalesID = S_SE_SD_SalesID
				 inner join B_GoodsInfo on S_SE_SD_SalesItemID = B_GI_GoodsID
				 left join S_ME_CustomerInfo on S_SE_SB_CustomerID = S_ME_CI_CustomerID
				 left join B_Supplier on B_GI_SupplierID = B_SP_ID
				 left join B_Brand on B_GI_BrandID = B_BD_ID and B_GI_SupplierID = B_BD_SupplierID
		  where S_SE_SB_ValueFlag = '1' 
				and S_SE_SB_WithdrawFlag = '1'
				and isnull(S_SE_SD_ItemID,'') <> ''
				and S_SE_SB_CheckoutFlag = '0'
				and S_SE_SD_CommoditiesFlag in ('4','5')
				and convert(varchar(10),S_SE_SB_WithdrawDate,120) >= '${beg}'
				and convert(varchar(10),S_SE_SB_WithdrawDate,120) <= '${end}'
				and ((ISNULL('${cid}','')<>'' AND B_GI_GoodsCategoryID = '${cid}') OR (ISNULL('${cid}','')=''))
				and ((ISNULL('${sid}','')<>'' AND B_GI_SupplierID = '${sid}') OR (ISNULL('${sid}','')=''))
				and ((ISNULL('${bid}','')<>'' AND B_GI_BrandID = '${bid}') OR (ISNULL('${bid}','')=''))
				and ((ISNULL('${batch}','')<>'' AND S_SE_SD_Batch like '%'+'${batch}'+'%') OR (ISNULL('${batch}','')=''))
				and ((ISNULL('${departmentid}','')<>'' AND S_SE_SB_ShopCode in (select departmentID from #salesDepartments)) OR (ISNULL('${departmentid}','')=''))
		  group by	S_SE_SD_ItemID,
					convert(varchar(10),S_SE_SB_PosDatetime,120),
					convert(varchar(10),S_SE_SD_Guaranteeperiod,120),
					S_SE_SD_Batch,
					S_ME_CI_Name
					,S_ME_CI_Phone
					,B_GI_GoodsName
					,B_GI_Spec
					,B_SP_SupplierName,B_BD_RegistrationNum
)temp 
group by 
	PosDatetime,
	cName,
	Phone,
	GoodsName,
	SupplierName,
	Batch,
	Spec,
	GuaranteeperiodDatetime,
	barcode,
	RegistrationNum

drop table #salesDepartments]]></Query>
</TableData>
