<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="queryDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
select C_ST_I_BillID,C_ST_I_billDate as C_ST_I_billDate,C_ST_I_AuditDate as C_ST_I_AuditDate,
       C_ST_I_FinanceAuditDate as C_ST_I_FinanceAuditDate,B_GC_GoodsCategoryName as B_GC_GoodsCategoryName,
       C_ST_I_SupplierId as C_ST_I_SupplierId,B_SP_SupplierName as B_SP_SupplierName,
       C_ST_IE_GoodsQuantity as C_ST_IE_GoodsQuantity,
       C_ST_IE_NotTaxRateAmount as C_ST_IE_NotTaxRateAmount,
       C_ST_IE_TaxAmount as C_ST_IE_TaxAmount,
       C_ST_IE_CostPriceAmount as C_ST_IE_CostPriceAmount,
       C_ST_IE_TaxRate as C_ST_IE_TaxRate,
       createperson as createperson,
       auditperson as auditperson
  from (
select C_ST_I_BillID,convert(varchar(10),C_ST_I_billDate,120) as C_ST_I_billDate,
	   convert(varchar(10),C_ST_I_AuditDate,120) as C_ST_I_AuditDate,convert(varchar(10),C_ST_I_FinanceAuditDate,120) as C_ST_I_FinanceAuditDate,      
	   B_GC_GoodsCategoryName as B_GC_GoodsCategoryName,C_ST_I_SupplierId as C_ST_I_SupplierId,B_SP_SupplierName as B_SP_SupplierName,
       sum(C_ST_IE_GoodsQuantity) as C_ST_IE_GoodsQuantity,
       sum(C_ST_IE_NotTaxRateAmount) as C_ST_IE_NotTaxRateAmount,
       sum(C_ST_IE_TaxAmount) as C_ST_IE_TaxAmount,
       sum(C_ST_IE_CostPriceAmount) as C_ST_IE_CostPriceAmount,C_ST_IE_TaxRate as C_ST_IE_TaxRate,
       a.personname as createperson,b.personname as auditperson
  from C_ST_Inventory inner join C_ST_InventoryEntry on C_ST_I_BillID=C_ST_IE_BillID
    left join B_Supplier on C_ST_I_SupplierId=B_SP_ID
    left join SYS_PersonInfo a on a.id=C_ST_I_createPerson
    left join SYS_PersonInfo b on b.id=C_ST_I_AuditPerson
    left join SYS_PersonInfo c on c.id=C_ST_I_FinanceAuditPerson
    left join B_GoodsInfo on C_ST_IE_GoodsId=B_GI_GoodsID
    left join B_GoodsCategory on B_GC_ID=B_GI_GoodsCategoryID
  where C_ST_I_BillTypeId in ('1','9') and C_ST_I_AuditState='1' and C_ST_I_FinanceAuditState='1'
        and convert(varchar(7),C_ST_I_FinanceAuditDate,120)='${queryDate}'
  group by C_ST_I_BillID,C_ST_I_billDate,C_ST_I_AuditDate,
           C_ST_I_FinanceAuditDate, B_GC_GoodsCategoryName,C_ST_I_SupplierId,
           B_SP_SupplierName,C_ST_IE_TaxRate,a.personname,b.personname
union all
select C_ST_I_BillID,convert(varchar(10),C_ST_I_billDate,120) as C_ST_I_billDate,
	   convert(varchar(10),C_ST_I_AuditDate,120) as C_ST_I_AuditDate,convert(varchar(10),C_ST_I_FinanceAuditDate,120) as C_ST_I_FinanceAuditDate,      
	   B_GC_GoodsCategoryName as B_GC_GoodsCategoryName,C_ST_I_SupplierId as C_ST_I_SupplierId,B_SP_SupplierName as B_SP_SupplierName,
       -sum(C_ST_IE_GoodsQuantity) as C_ST_IE_GoodsQuantity,
       -sum(C_ST_IE_NotTaxRateAmount) as C_ST_IE_NotTaxRateAmount,
       -sum(C_ST_IE_TaxAmount) as C_ST_IE_TaxAmount,
       -sum(C_ST_IE_CostPriceAmount) as C_ST_IE_CostPriceAmount,C_ST_IE_TaxRate as C_ST_IE_TaxRate,
       a.personname as createperson,b.personname as auditperson
  from C_ST_Inventory inner join C_ST_InventoryEntry on C_ST_I_BillID=C_ST_IE_BillID
    left join B_Supplier on C_ST_I_SupplierId=B_SP_ID
    left join SYS_PersonInfo a on a.id=C_ST_I_createPerson
    left join SYS_PersonInfo b on b.id=C_ST_I_AuditPerson
    left join SYS_PersonInfo c on c.id=C_ST_I_FinanceAuditPerson
    left join B_GoodsInfo on C_ST_IE_GoodsId=B_GI_GoodsID
    left join B_GoodsCategory on B_GC_ID=B_GI_GoodsCategoryID
  where C_ST_I_BillTypeId in ('2') and C_ST_I_AuditState='1' and C_ST_I_FinanceAuditState='1'
        and convert(varchar(7),C_ST_I_FinanceAuditDate,120)='${queryDate}'
  group by C_ST_I_BillID,C_ST_I_billDate,C_ST_I_AuditDate,
           C_ST_I_FinanceAuditDate, B_GC_GoodsCategoryName,C_ST_I_SupplierId,
           B_SP_SupplierName,C_ST_IE_TaxRate,a.personname,b.personname

)temp
  order by C_ST_I_FinanceAuditDate





]]></Query>
</TableData>
