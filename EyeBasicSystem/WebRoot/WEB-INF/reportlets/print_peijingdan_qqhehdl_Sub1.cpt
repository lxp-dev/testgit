<TableData name="商品信息" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="salesID"/>
<O>
<![CDATA[X0073620140820150509]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT TOP 5
--------关联商品表,取规格,工艺类型 begin--------------
CASE
  WHEN B_GI_GoodsCategoryID = '1' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_Color), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN B_GI_GoodsCategoryID = '6' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_Color), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN B_GI_GoodsCategoryID = '8' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_SupplierColor), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN ( B_GI_GoodsCategoryID = '4'
          OR B_GI_GoodsCategoryID = '5' )
       AND F_SP_PrintStealthCheck = '1' THEN ( B_GI_GoodsName + '：' + Isnull(S_SE_SD_Guaranteeperiod, '') )
  ELSE Isnull(B_GI_GoodsName, '')
END                                         AS '商品名称',
--------关联商品表,取规格,工艺类型 end--------------
Isnull(SUM(Isnull(S_SE_SD_Number, 0)), 0)   AS '商品数量',
Isnull(S_SE_SD_SPrice, 0)                   AS '商品单价',
Isnull(SUM(Isnull(S_SE_SD_PriceSum, 0)), 0) AS '商品原价',
Isnull(S_SE_SD_SalesValue, 0)               AS '商品应收金额',
Isnull(S_SE_SB_SalesValue, 0)               AS '商品总计金额',
Isnull(B_GI_UnitId, 0)                      AS '计量单位',
S_SE_SD_DiscountRate                        AS '折扣率'
FROM   F_SystemParameter,
       uview_SalesBasic
       INNER JOIN uview_SalesDetail
         ON S_SE_SB_SalesID = S_SE_SD_SalesID
       -------关联商品表,取规格,工艺类型 add by zk begin------------
       LEFT JOIN B_GoodsInfo
         ON B_GI_GoodsID = uview_SalesDetail.S_SE_SD_SalesItemID
       LEFT JOIN F_TechnologyType
         ON B_GoodsInfo.B_GI_frameProcessCraftType = F_TechnologyType.F_TT_ID
-------关联商品表,取规格,工艺类型 add by zk enb------------
WHERE  S_SE_SB_SalesID = '${salesID}'
       AND S_SE_SB_ValueFlag = '1'
       AND Isnull(S_SE_SD_LargessFlag, '0') = '0'
GROUP  BY S_SE_SD_SalesItemID,
          B_GI_GoodsName,
          S_SE_SD_SPrice,
          B_GI_Spec,
          F_TT_Name,
          B_GI_GoodsCategoryID,
          B_GI_Color,
          B_GI_SupplierColor,
          S_SE_SD_DiscountRate,
          S_SE_SD_Guaranteeperiod,
          F_SP_PrintStealthCheck,
          B_GI_UnitId,
          S_SE_SD_SalesValue,
          S_SE_SB_SalesValue]]></Query>
</TableData>
