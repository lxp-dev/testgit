<TableDataMap>
<TableData name="赠品" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="salesID"/>
<O>
<![CDATA[X0073620140820150509]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT TOP 8 Isnull(S_SE_SD_SalesItemName, '')           AS '赠品名称',
             Isnull(SUM(Isnull(S_SE_SD_Number, 0)), 0)   AS '赠品数量',
             Isnull(S_SE_SD_SPrice, 0)                   AS '赠品单价',
             Isnull(SUM(Isnull(S_SE_SD_PriceSum, 0)), 0) AS '赠品金额合计'
FROM   uview_SalesBasic
       INNER JOIN uview_SalesDetail
         ON S_SE_SB_SalesID = S_SE_SD_SalesID
WHERE  S_SE_SB_SalesID = '${salesID}'
       AND S_SE_SB_ValueFlag = '1'
       AND Isnull(S_SE_SD_LargessFlag, '0') = '1'
GROUP  BY S_SE_SD_SalesItemID,
          S_SE_SD_SalesItemName,
          S_SE_SD_SPrice
ORDER  BY S_SE_SD_SalesItemID ]]></Query>
</TableData>
<TableData name="加工要求" class="com.fr.data.impl.DBTableData">
