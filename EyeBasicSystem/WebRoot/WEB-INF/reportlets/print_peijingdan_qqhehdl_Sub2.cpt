<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT S_SE_SD_ID          AS 'ID',
       S_SE_SD_SalesID     AS '销售单号',
       Isnull(S_SE_SD_Requirement, '') AS '特殊加工要求'
FROM   S_SE_SpecialPDetail
WHERE  S_SE_SD_SalesID = '${salesid}' ]]></Query>
</TableData>
