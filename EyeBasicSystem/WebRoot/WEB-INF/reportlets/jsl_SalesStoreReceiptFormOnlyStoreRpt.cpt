<TableData name="ds3" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="shopCode"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="bgnDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="endDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
declare @bgnDate varchar(10),@endDate varchar(10),@shopCode varchar(4000)
set @bgnDate='${bgnDate}'
set @endDate='${endDate}'
set @shopCode='${shopCode}'

IF (object_id('tempdb..#saleslog') IS NOT NULL)
  drop table #saleslog
IF (object_id('tempdb..#salesDepartments') IS NOT NULL)
  drop table #salesDepartments

select departmentID into #salesDepartments from sales_createShopCodeTab(@shopCode)

select B_DP_DepartmentName as dptname,S_SE_SB_PosID as posid,a.personname as posname,
       S_SE_SL_PaymentType as paytype,S_SE_SL_ConsumptionID as conid,
       isnull(S_SE_SL_Price,0) as price,0 as arramount  into #saleslog
  from S_SE_SalesBasic inner join S_SE_SalesLog on S_SE_SL_SalesID=S_SE_SB_SalesID
       left join SYS_PersonInfo a on a.id=S_SE_SB_PosID
       left join B_Departments on B_DP_DepartmentID=S_SE_SB_ShopCode
  where S_SE_SB_ValueFlag='1' 
        and ((isnull(@shopCode,'')<>'' and S_SE_SB_ShopCode in (select departmentID from #salesDepartments)) or isnull(@shopCode,'')='')      
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SL_DateTime,120)>=@bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SL_DateTime,120)<=@endDate) or isnull(@endDate,'')='')


select dptname as dptname,posid as posid,posname as posname,a1,(a2-a5) as a2,a3,a4,a5 from (
select dptname as dptname,posid as posid,posname as posname,isnull(sum(isnull(cast(a1 as numeric(30,2)),0)),0) as a1,isnull(sum(isnull(cast(a2 as numeric(30,2)),0)),0) as a2,
       isnull(sum(isnull(cast(a3 as numeric(30,2)),0)),0) as a3,isnull(sum(isnull(cast(a4 as numeric(30,2)),0)),0) as a4,
       isnull(sum(isnull(cast(a1 as numeric(30,2)),0)+isnull(cast(a3 as numeric(30,2)),0)+isnull(cast(a4 as numeric(30,2)),0)),0) as a5
 from (
-- 全款(现金)
select dptname as dptname,posid as posid,posname as posname,cast(price as numeric(30,2)) as a1,0 as a2,0 as a3,0 as a4 from #saleslog where paytype='1' and conid='1'
union all 
-- 订金(现金)
select dptname as dptname,posid as posid,posname as posname,cast(price as numeric(30,2)) as a1,arramount as a2,0 as a3,0 as a4 from #saleslog where paytype='2' and conid='1'
union all
-- 补齐(现金)
select dptname as dptname,posid as posid,posname as posname,cast(price as numeric(30,2)) as a1,-cast(price as numeric(30,2)) as a2,0 as a3,0 as a4 from #saleslog where paytype='3' and conid='1'
union all
-- 退款(现金)
select dptname as dptname,posid as posid,posname as posname,-cast(price as numeric(30,2)) as a1,0 as a2,0 as a3,0 as a4 from #saleslog where paytype='4' and conid='1'
union all
-- 找零(现金)
select dptname as dptname,posid as posid,posname as posname,-cast(price as numeric(30,2)) as a1,0 as a2,0 as a3,0 as a4 from #saleslog where conid='5'
union all
-- 全款(银行卡)
select dptname as dptname,posid as posid,posname as posname,0 as a1,0 as a2,cast(price as numeric(30,2)) as a3,0 as a4 from #saleslog where paytype='1' and conid='3'
union all
-- 订金(银行卡)
select dptname as dptname,posid as posid,posname as posname,0 as a1,arramount as a2,cast(price as numeric(30,2)) as a3,0 as a4 from #saleslog where paytype='2' and conid='3'
union all
-- 补齐(银行卡)
select dptname as dptname,posid as posid,posname as posname,0 as a1,-cast(price as numeric(30,2)) as a2,cast(price as numeric(30,2)) as a4,0 as a4 from #saleslog where paytype='3' and conid='3'
union all
-- 全款(储值卡)
select dptname as dptname,posid as posid,posname as posname,0 as a1,0 as a2,0 as a3,cast(price as numeric(30,2)) as a4 from #saleslog where paytype='1' and conid='4'
union all
-- 订金(储值卡)
select dptname as dptname,posid as posid,posname as posname,0 as a1,arramount as a2,0 as a3,cast(price as numeric(30,2)) as a4 from #saleslog where paytype='2' and conid='4'
union all
-- 补齐(储值卡)
select dptname as dptname,posid as posid,posname as posname,0 as a1,-cast(price as numeric(30,2)) as a2,0 as a3,cast(price as numeric(30,2)) as a4 from #saleslog where paytype='3' and conid='4'
union all
-- 应收金额
select B_DP_DepartmentName as dptname,S_SE_SB_PosID as posid,a.personname as posname,0 as a1,S_SE_SB_SalesValue as a2,0 as a3,0 as a4 
   from S_SE_SalesBasic left join SYS_PersonInfo a on a.id=S_SE_SB_PosID
       left join B_Departments on B_DP_DepartmentID=S_SE_SB_ShopCode
  where S_SE_SB_ValueFlag='1' 
        and ((isnull(@shopCode,'')<>'' and S_SE_SB_ShopCode in (select departmentID from #salesDepartments)) or isnull(@shopCode,'')='')      
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SB_PosDatetime,120)>=@bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SB_PosDatetime,120)<=@endDate) or isnull(@endDate,'')='')
union all
-- 退应收金额
select B_DP_DepartmentName as dptname,S_SE_SB_WithdrawPersonID as posid,a.personname as posname,0 as a1,-S_SE_SB_SalesValue as a2,0 as a3,0 as a4 
   from S_SE_SalesBasic left join SYS_PersonInfo a on a.id=S_SE_SB_WithdrawPersonID
       left join B_Departments on B_DP_DepartmentID=S_SE_SB_ShopCode
  where S_SE_SB_ValueFlag='1' and S_SE_SB_WithdrawFlag='1'
        and ((isnull(@shopCode,'')<>'' and S_SE_SB_ShopCode in (select departmentID from #salesDepartments)) or isnull(@shopCode,'')='')      
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SB_WithdrawDate,120)>=@bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SB_WithdrawDate,120)<=@endDate) or isnull(@endDate,'')='')


)temp 
  group by dptname,posid,posname
)temp
  order by dptname,a5 desc

drop table #saleslog
drop table #salesDepartments

]]></Query>
</TableData>
