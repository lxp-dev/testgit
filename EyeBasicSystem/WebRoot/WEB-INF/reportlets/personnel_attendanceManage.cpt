<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="selPersonName"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="selmonth"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="selDepartmentID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT M_AM_UUID        AS mamuuid,
       M_AM_PersonId    AS mampersonid,
       mamdepartmentname=Stuff((SELECT ',' + B_DP_DepartmentName
                                FROM   SYS_PersonDepartments,
                                       B_Departments
                                WHERE  SYS_PersonInfo.ID = SYS_PD_PersonID
                                       AND SYS_PD_DepartmentID = B_DP_DepartmentID
                                FOR XML PATH('')), 1, 1, ''),
       personName       AS mampersonname,
       M_AM_chidao      AS mamchidao,
       M_AM_zaotui      AS mamzaotui,
       M_AM_bingjia     AS mambingjia,
       M_AM_shijia      AS mamshijia,
       M_AM_hunjia      AS mamhunjia,
       M_AM_sangjia     AS mamsangjia,
       M_AM_chanjia     AS mamchanjia,
       M_AM_tanqinjia   AS mamtanqinjia,
       M_AM_gongjia     AS mamgongjia,
       M_AM_chuchai     AS mamchuchai,
       M_AM_waichuxuexi AS mamwaichuxuexi,
       '考勤日期：'+M_AM_year+M_AM_month        AS kaoqinriqi
FROM   M_AttendanceManage
       LEFT JOIN SYS_PersonInfo
         ON M_AttendanceManage.M_AM_PersonId = SYS_PersonInfo.id
WHERE  userState = 0 
${if(len(selId)==0,""," and SYS_PersonInfo.id='"+selId+"'") }
${if(len(selPersonName)==0,""," and personName='"+selPersonName+"'") }
${if(len(selyear)==0,""," and M_AM_Year='"+selyear+"'") }
${if(len(selmonth)==0,""," and M_AM_Month='"+selmonth+"'") }
${if(len(selDepartmentID)==0,""," and sys_personinfo.id in(select SYS_PD_PersonID from SYS_PersonDepartments where sys_pd_departmentid='"+selDepartmentID+"')")}
]]></Query>
</TableData>
