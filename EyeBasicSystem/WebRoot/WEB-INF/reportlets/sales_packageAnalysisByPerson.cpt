<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="shopCode"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="packageType"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="bgnDate"/>
<O>
<![CDATA[2013-02-04]]></O>
</Parameter>
<Parameter>
<Attributes name="packageId"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="endDate"/>
<O>
<![CDATA[2014-02-04]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[DECLARE @departmentid VARCHAR(1000)
SET @departmentid = '${shopCode}'

IF(@departmentid = 'null')
BEGIN
  SET @departmentid = ''
END

SELECT departmentID
INTO   #salesDepartments
FROM   Sales_createshopcodetab(@departmentid)

SELECT 
       SUM(rc)     AS rc,
       SUM(trc)   AS trc,
        S_SE_SB_SetMealID,
        S_SM_SM_Title
FROM   (

        SELECT
        COUNT(S_SE_SB_SalesID) AS rc,
        0 AS trc,
        S_SE_SB_CheckoutFlag,
        S_SE_SB_WithdrawFlag,
        S_SE_SB_SetMealID,
        S_SM_SM_Title
        FROM   S_SE_SalesBasic
               INNER JOIN S_SM_SetMeal
                 ON S_SM_SM_ID = S_SE_SB_SetMealID
        WHERE  Isnull(S_SE_SB_WithdrawFlag, 0) <> 1
               AND (ISNULL('${packageId}', '') = '' OR ISNULL('${packageId}', '') = S_SE_SB_SetMealID)
               AND (
                       (ISNULL('${packageType}', '') = '' OR ISNULL('${packageType}', '') = '1' AND S_SE_SB_OrdersType IN ('1', '2'))
                       OR (ISNULL('${packageType}', '') = '' OR ISNULL('${packageType}', '') = '2' AND S_SE_SB_OrdersType IN ('3', '4'))
                       OR (ISNULL('${packageType}', '') = '' OR ISNULL('${packageType}', '') = '3' AND S_SE_SB_OrdersType = '5')
                    )
              AND (ISNULL('${bgnDate}', '') = '' OR ISNULL('${bgnDate}', '') <= CONVERT(VARCHAR(10), S_SE_SB_SalesDatetime, 23))
              AND (ISNULL('${endDate}', '') = '' OR ISNULL('${endDate}', '') >= CONVERT(VARCHAR(10), S_SE_SB_SalesDatetime, 23))
              AND (ISNULL(@departmentid, '')='' OR S_SE_SB_ShopCode IN (SELECT departmentID
                                                      FROM   #salesDepartments))
GROUP BY 
        S_SE_SB_CheckoutFlag,
        S_SE_SB_WithdrawFlag,
        S_SE_SB_SetMealID,
        S_SM_SM_Title

        UNION ALL

        SELECT
        0,
        COUNT(S_SE_SB_SalesID),
        S_SE_SB_CheckoutFlag,
        S_SE_SB_WithdrawFlag,
        S_SE_SB_SetMealID,
        S_SM_SM_Title
        FROM   S_SE_SalesBasic
               INNER JOIN S_SM_SetMeal
                 ON S_SM_SM_ID = S_SE_SB_SetMealID
        WHERE  Isnull(S_SE_SB_WithdrawFlag, 0) = 1
               AND (ISNULL('${packageId}', '') = '' OR ISNULL('${packageId}', '') = S_SE_SB_SetMealID)
               AND (
                       (ISNULL('${packageType}', '') = '' OR ISNULL('${packageType}', '') = '1' AND S_SE_SB_OrdersType IN ('1', '2'))
                       OR (ISNULL('${packageType}', '') = '' OR ISNULL('${packageType}', '') = '2' AND S_SE_SB_OrdersType IN ('3', '4'))
                       OR (ISNULL('${packageType}', '') = '' OR ISNULL('${packageType}', '') = '3' AND S_SE_SB_OrdersType = '5')
                    )
              AND (ISNULL('${bgnDate}', '') = '' OR ISNULL('${bgnDate}', '') <= CONVERT(VARCHAR(10), S_SE_SB_SalesDatetime, 23))
              AND (ISNULL('${endDate}', '') = '' OR ISNULL('${endDate}', '') >= CONVERT(VARCHAR(10), S_SE_SB_SalesDatetime, 23))
              AND (ISNULL(@departmentid, '')='' OR S_SE_SB_ShopCode IN (SELECT departmentID
                                                      FROM   #salesDepartments))
GROUP BY 
        S_SE_SB_CheckoutFlag,
        S_SE_SB_WithdrawFlag,
        S_SE_SB_SetMealID,
        S_SM_SM_Title

) t1
GROUP  BY 
        S_SE_SB_SetMealID,
        S_SM_SM_Title
ORDER BY SUM(rc) DESC 


DROP TABLE #salesDepartments 
]]></Query>
</TableData>
