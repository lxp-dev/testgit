<TableData name="ds3" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="shopCode"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="bgnDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="endDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT departmentID
INTO   #salesDepartments
FROM   Sales_createshopcodetab('${shopCode}')

SELECT posid,
       posname,
       SUM(tuihuo)   AS tuihuo,
       SUM(xiaoshou) AS xiaoshou
FROM   (SELECT S_SE_SB_ShopCode                              AS posid,
               B_DP_DepartmentName                           AS posname,
               0                                             AS tuihuo,
               Isnull(SUM(Isnull(S_SE_SB_SalesValue, 0)), 0) AS xiaoshou
        FROM   uview_SalesBasic
               LEFT JOIN B_Departments
                 ON S_SE_SB_ShopCode = B_DP_DepartmentID
        WHERE  S_SE_SB_ValueFlag = '1'
               AND ( ( Isnull('${shopCode}', '') <> ''
                       AND S_SE_SB_ShopCode IN (SELECT departmentID
                                                FROM   #salesDepartments) )
                      OR Isnull('${shopCode}', '') = '' )
               AND ( ( Isnull('${bgnDate}', '') <> ''
                       AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 120) >= '${bgnDate}' )
                      OR Isnull('${bgnDate}', '') = '' )
               AND ( ( Isnull('${endDate}', '') <> ''
                       AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 120) <= '${endDate}' )
                      OR Isnull('${endDate}', '') = '' )
        GROUP  BY S_SE_SB_ShopCode,
                  B_DP_DepartmentName
        UNION ALL
        SELECT S_SE_SB_ShopCode                               AS posid,
               B_DP_DepartmentName                            AS posname,
               -Isnull(SUM(Isnull(S_SE_SB_SalesValue, 0)), 0) AS tuihuo,
               0                                              AS xiaoshou
        FROM   uview_SalesBasic
               LEFT JOIN B_Departments
                 ON S_SE_SB_ShopCode = B_DP_DepartmentID
        WHERE  S_SE_SB_ValueFlag = '1'
               AND S_SE_SB_WithdrawFlag = '1'
               AND ( ( Isnull('${shopCode}', '') <> ''
                       AND S_SE_SB_ShopCode IN (SELECT departmentID
                                                FROM   #salesDepartments) )
                      OR Isnull('${shopCode}', '') = '' )
               AND ( ( Isnull('${bgnDate}', '') <> ''
                       AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 120) >= '${bgnDate}' )
                      OR Isnull('${bgnDate}', '') = '' )
               AND ( ( Isnull('${endDate}', '') <> ''
                       AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 120) <= '${endDate}' )
                      OR Isnull('${endDate}', '') = '' )
        GROUP  BY S_SE_SB_ShopCode,
                  B_DP_DepartmentName)TEMP
GROUP  BY posid,
          posname
ORDER  BY posid

DROP TABLE #salesDepartments ]]></Query>
</TableData>
