<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="billID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT TOP 1 C_SHA_N_billID       AS cshanbillid, --单号
			 C_SHA_N_DepartmentID AS cshandepartmentid, --部门ID
			 B_DP_DepartmentName  AS cshandepartmentname, --部门名称
			 C_SHA_N_createPerson AS cshancreateperson, --操作人ID
			 a.personName         AS cshancreatepersonname, --操作人
			 b.personName         AS cshanauditpersonname, --审核人
			 C_SHA_N_createDate   AS cshancreatedate, --单据日期
			 C_SHA_N_AuditPerson  AS cshanauditperson, --审核人ID
			 C_SHA_N_AuditDate    AS cshanauditdate, --审核日期
			 (case 
				when (C_SHA_N_AuditState = '0') then '未审核'
				when (C_SHA_N_AuditState = '1') then '已审核'
			 end) AS cshanauditstate, --审核状态
			 C_SHA_N_consignMode  AS cshanconsignmode,
             C_SHA_N_Responsibility as zrrid,
             c.personName as zrrname 
FROM   C_SHA_Nonconforming
	   INNER JOIN B_Departments
		 ON C_SHA_N_DepartmentID = B_DP_DepartmentID
	   LEFT JOIN (SELECT ID,
						 personName
				  FROM   SYS_PersonInfo)a
		 ON C_SHA_N_createPerson = a.ID
	   LEFT JOIN (SELECT ID,
						 personName
				  FROM   SYS_PersonInfo)b
		 ON C_SHA_N_AuditPerson = b.ID
              left join SYS_PersonInfo c on C_SHA_N_Responsibility = c.id
WHERE  C_SHA_N_billID = '${billID}']]></Query>
</TableData>
<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="billID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT          S_SE_SD_GlassFlag            AS leftorright,
                C_SHA_NE_billID              AS cshanebillid,
                C_SHA_NE_goodsID             AS cshanegoodsid,
                isnull(C_SHA_NE_BarCode,'')+' '             AS cshanebarcode,
                C_SHA_NE_goodsName           AS cshanegoodsname,
                C_SHA_NE_goodsType           AS cshanegoodstype,
                C_SHA_NE_reasons1            AS cshanereasons1,
                C_SHA_NE_reasons2            AS cshanereasons2,
                a.F_NP_content               AS cshanereasons1name,
                b.F_NP_content               AS cshanereasons2name,
                C_SHA_NE_remark              AS cshaneremark,
	      C_SHA_NE_GoodsQuantity	    as quantity,
case
when C_SHA_NE_consignMode='0' then '报残'        
when C_SHA_NE_consignMode='1' then '退回'
end AS cshaneconsignmode,
case
when B_GoodsInfo.B_GI_isCustomize='D' then '订做片'
when B_GoodsInfo.B_GI_isCustomize='0' then '成品片'
end AS iscustomize,
 S_SE_SD_SalesValue           AS cshanesalesvalue
FROM   C_SHA_NonconformingEntry
       INNER JOIN C_SHA_Nonconforming
         ON C_SHA_NE_billID = C_SHA_N_billID
       INNER JOIN B_GoodsInfo
         ON C_SHA_NE_goodsID = B_GI_GoodsID
       LEFT JOIN (SELECT F_NP_ID,
                         F_NP_content
                  FROM   F_NonconformingProduct)a
         ON C_SHA_NE_reasons1 = a.F_NP_ID
       LEFT JOIN (SELECT F_NP_ID,
                         F_NP_content
                  FROM   F_NonconformingProduct)b
         ON C_SHA_NE_reasons2 = b.F_NP_ID
       LEFT OUTER JOIN uview_SalesDetail
         ON C_SHA_N_linkBillID = S_SE_SD_SalesID
            AND C_SHA_NE_SalesDetailID = S_SE_SD_ID
WHERE  C_SHA_NE_billID = '${billID}'
order by S_SE_SD_GlassFlag desc]]></Query>
</TableData>
