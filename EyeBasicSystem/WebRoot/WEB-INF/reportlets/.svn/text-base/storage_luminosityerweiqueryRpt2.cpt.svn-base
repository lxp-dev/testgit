<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[DECLARE @goodsCategoryID VARCHAR(10),
        @supplierID      VARCHAR(10),
        @brandID         VARCHAR(10),
        @warehouseID     VARCHAR(10),
        @enddate         datetime,
        @startdate     datetime

set @goodsCategoryID='${goodsCategoryID}'
set @supplierID='${supplierID}'
set @brandID='${brandID}'
set @warehouseID='${warehouseID}'
set @enddate='${enddate}'
set @startdate='${startdate}'


select sum(goodsNum) as goodsNumSum from (
SELECT B_GI_Sph AS sph,
       B_GI_Cyl AS cyl,
       B_GI_GoodsID  AS goodsID,
       isnull(goodsNum,0) AS goodsNum
FROM   B_GoodsInfo
	LEFT JOIN(
		SELECT S_SE_SD_SalesItemID AS goodsID,
               SUM(S_SE_SD_Number) AS goodsNum
        FROM   uview_SalesDetail
       left join uview_SalesBasic on S_SE_SB_SalesID=S_SE_SD_SalesID
        WHERE  S_SE_SD_StockId = @warehouseID and S_SE_SB_ValueFlag=1
	and ((isnull(@enddate,'') <> '' and convert(varchar(10),S_SE_SB_PosDatetime,23) <=@enddate) or (isnull(@enddate,'')=''))	
and ((isnull(@startdate,'') <> '' and convert(varchar(10),S_SE_SB_PosDatetime,23) >=@startdate) or (isnull(@startdate,'')=''))
        GROUP  BY S_SE_SD_SalesItemID,
                  S_SE_SD_StockId
	)TEMP
        
         ON B_GI_GoodsID=goodsID
		left join B_Supplier on B_SP_ID=B_GI_SupplierID
WHERE  B_GI_isCustomize = '0'
       AND B_GI_SupplierID <> 'ZZ' 
	  AND   B_GI_GoodsCategoryID= @goodsCategoryID
               AND B_GI_SupplierID = @supplierID
               AND B_GI_BrandID = @brandID
 and ((isnull('${makeinvoiceflags}','') = '2' and B_SP_MakeInvoiceFlag ='${makeinvoiceflag }') or (isnull('${makeinvoiceflag}','')=''))
) as temp]]></Query>
</TableData>
