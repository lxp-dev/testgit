<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="invoiceID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select L_PB_PB_ID as pbID,
       (case isnull(L_IT_Type,'') when '' then (
           case (select top 1 C_ST_I_BillTypeId from C_ST_Inventory where C_ST_I_BillID = L_PB_PBE_InvoiceID)
              when '1' then '采购收货单'
              when '2' then '采购退货单'
              when '9' then '委外收货单'
             else '' end
       ) else isnull(L_IT_Type,'') end) as billType,
       L_PB_PBE_InvoiceID as invoiceID,convert(varchar(10),L_PB_PB_Date,120) as createDate,
       B_SP_SupplierName as supplierName,a.personName as createPersonName,b.personName as auditPersonName,
       convert(varchar(10),L_PB_PB_AuditDate,120) as auditDate,B_DP_DepartmentName as departmentName,
       sum(L_PB_PBE_PayMentAmount) as payMentAmount
  from L_PB_PBE_PaymentBillEntry inner join L_PB_PB_PaymentBill on L_PB_PBE_BillID=L_PB_PB_ID
       left join L_I_Invoice on L_PB_PBE_InvoiceID=L_I_I_ID
       left join L_IT_InvoiceType on L_I_I_InvoiceType=L_IT_ID
       left join SYS_PersonInfo a on L_PB_PB_CreatePersonID=a.id
       left join SYS_PersonInfo b on L_PB_PB_CreatePersonID=b.id
       left join B_Departments on L_PB_PB_DepartmentID=B_DP_DepartmentID
       left join B_Supplier on L_PB_PB_SupplierID=B_SP_ID

  where L_PB_PB_ID='${invoiceID}'
  
group by L_PB_PB_ID,L_IT_Type,L_PB_PBE_InvoiceID,L_PB_PB_Date,B_SP_SupplierName,a.personName,b.personName,L_PB_PB_AuditDate,B_DP_DepartmentName]]></Query>
</TableData>
