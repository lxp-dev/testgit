</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select departmentID into #shopCode from sales_createShopCodeTab('${departmentsID}')
select salesArea into #salesarea from sales_createSalesAreaTab('${priceArea}','${reportID}')
select convert(varchar(10),R_SD_DCAE_Date,120) as DCAE_Date,R_SD_DCAE_AreaID,
isnull(sum(R_SD_DCAE_SalesNum),0) as SalesNum,
isnull(sum(R_SD_DCAE_SalesAmount),0.00) as SalesAmount
from R_SD_DayCollectAreaEntry
where 1=1
and ((isnull('${bgnDate}','')<>'' and convert(varchar(10),R_SD_DCAE_Date,120)>='${bgnDate}') or (isnull('${bgnDate}','')=''))
and ((isnull('${endDate}','')<>'' and convert(varchar(10),R_SD_DCAE_Date,120)<='${endDate}') or (isnull('${endDate}','')=''))
and ((isnull('${departmentsID}','')<>'' 
and R_SD_DCAE_ShopCode in (select departmentID from #shopCode )) or (isnull('${departmentsID}','')=''))
and ((isnull('${priceArea}','')<>'' and R_SD_DCAE_AreaID in (select salesarea from #salesarea )) or (isnull('${priceArea}','')=''))
and ((isnull('${salesType}','')<>'' and R_SD_DCAE_SalesType='${salesType}') or (isnull('${salesType}','')=''))
group by R_SD_DCAE_AreaID,R_SD_DCAE_Date
order by cast(substring(R_SD_DCAE_AreaID,1,CHARINDEX('-',R_SD_DCAE_AreaID)-1) as float),R_SD_DCAE_Date

drop table #shopCode
drop table #salesarea]]></Query>
</TableData>
