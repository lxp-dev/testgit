<Query>
<![CDATA[--小于600
SELECT TEMP.P_CH_GC_ProcessPersonID,
	   personName,
       TEMP.P_CH_GC_ProcessDepartment,
	   B_DP_DepartmentName,
       SUM(TEMP.a1) AS a1,
       SUM(TEMP.b1) AS b1,
       SUM(TEMP.c1) AS c1,
       SUM(TEMP.a2) AS a2,
       SUM(TEMP.b2) AS b2,
       SUM(TEMP.c2) AS c2,
       SUM(TEMP.a3) AS a3,
       SUM(TEMP.b3) AS b3,
       SUM(TEMP.c3) AS c3
FROM   (SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               COUNT(P_CH_GC_SalesId) AS a1,
               0                      AS b1,
               0                      AS c1,
               0                      AS a2,
               0                      AS b2,
               0                      AS c2,
               0                      AS a3,
               0                      AS b3,
               0                      AS c3
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'a'
               AND P_CH_GC_GlassesSalesValue < 600
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0                      AS a1,
               COUNT(P_CH_GC_SalesId) AS b1,
               0                      AS c1,
               0                      AS a2,
               0                      AS b2,
               0                      AS c3,
               0                      AS a3,
               0                      AS b3,
               0                      AS c3
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'b'
               AND P_CH_GC_GlassesSalesValue < 600
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0                      AS a1,
               0                      AS b1,
               COUNT(P_CH_GC_SalesId) AS c1,
               0                      AS a2,
               0                      AS b2,
               0                      AS c2,
               0                      AS a3,
               0                      AS b3,
               0                      AS c3
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'c'
               AND P_CH_GC_GlassesSalesValue < 600
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        --大于等于600小于1000
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0                      AS a1,
               0                      AS b1,
               0                      AS c1,
               COUNT(P_CH_GC_SalesId) AS a2,
               0                      AS b2,
               0                      AS c2,
               0                      AS a3,
               0                      AS b3,
               0                      AS c3
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'a'
               AND P_CH_GC_GlassesSalesValue < 1000
               AND P_CH_GC_GlassesSalesValue >= 600
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0,
               0,
               0,
               0,
               COUNT(P_CH_GC_SalesId) AS b2,
               0,
               0,
               0,
               0
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'b'
               AND P_CH_GC_GlassesSalesValue < 1000
               AND P_CH_GC_GlassesSalesValue >= 600
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0,
               0,
               0,
               0,
               0,
               COUNT(P_CH_GC_SalesId) AS c2,
               0,
               0,
               0
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'c'
               AND P_CH_GC_GlassesSalesValue < 1000
               AND P_CH_GC_GlassesSalesValue >= 600
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        --大于等于600小于1000
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0,
               0,
               0,
               0,
               0,
               0,
               COUNT(P_CH_GC_SalesId) AS a3,
               0,
               0
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'a'
               AND P_CH_GC_GlassesSalesValue >= 1000
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0,
               0,
               0,
               0,
               0,
               0,
               0,
               COUNT(P_CH_GC_SalesId) AS b3,
               0
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'b'
               AND P_CH_GC_GlassesSalesValue >= 1000
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment
        UNION ALL
        SELECT P_CH_GC_ProcessPersonID,
               P_CH_GC_ProcessDepartment,
               0,
               0,
               0,
               0,
               0,
               0,
               0,
               0,
               COUNT(P_CH_GC_SalesId) AS c3
        FROM   dbo.P_CH_GlassesCheck
        WHERE  P_CH_GC_dengji = 'c'
               AND P_CH_GC_GlassesSalesValue >= 1000
        and  convert(varchar(10),P_CH_GC_CheckDate,23) >= '${begintime}'
        and  convert(varchar(10),P_CH_GC_CheckDate,23) <= '${endtime}'
        ${if(len(dopersonid) == 0,"","and P_CH_GC_ProcessPersonID = '" + dopersonid + "'")}
        GROUP  BY P_CH_GC_ProcessPersonID,
                  P_CH_GC_ProcessDepartment) TEMP
inner join dbo.SYS_PersonInfo on id = TEMP.P_CH_GC_ProcessPersonID
inner join dbo.B_Departments  on B_DP_DepartmentID = TEMP.P_CH_GC_ProcessDepartment
GROUP  BY P_CH_GC_ProcessPersonID,
		  personName,
          P_CH_GC_ProcessDepartment,
		  B_DP_DepartmentName]]></Query>
