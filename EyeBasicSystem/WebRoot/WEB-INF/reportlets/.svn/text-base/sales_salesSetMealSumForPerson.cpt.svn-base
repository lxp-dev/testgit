<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT departmentID
INTO   #shopCode
FROM   Sales_createshopcodetab('${departmentID}')


SELECT  MealID,
       MealName,
       sum(num1) as num1,
       S_SE_SB_ShopCode,
       B_DP_DepartmentName,
       sum(num2) as num2
FROM   (

SELECT S_SE_SD_Setmealid               AS MealID,
               S_SM_SM_Title                   AS MealName,
               COUNT(DISTINCT S_SE_SD_SalesID) AS num1,
				S_SE_SB_ShopCode as S_SE_SB_ShopCode,
				B_DP_DepartmentName as B_DP_DepartmentName,
               0                               AS num2
        FROM   S_SE_SalesBasic
			   INNER join B_Departments
				 ON B_DP_DepartmentID=S_SE_SB_ShopCode

               INNER JOIN S_SE_SalesDetail
                 ON S_SE_SB_SalesID = S_SE_SD_SalesID

               INNER JOIN S_SM_SetMealRecord
                 ON S_SE_SD_Setmealid = S_SM_SM_ID
        where S_SE_SB_ValueFlag='1' 
	and convert(varchar(10),S_SE_SB_PosDatetime,23)>='${beginDate}'
	and convert(varchar(10),S_SE_SB_PosDatetime,23)<='${endDate}'
	and ((S_SE_SB_ShopCode in (select departmentID from #shopCode) and isnull('${departmentID}','')<>'') or isnull('${departmentID}','')='')
	and (S_SM_SM_Classify = '${ClassifyID}' or isnull('${ClassifyID}','') = '')
	and (S_SE_SD_Setmealid = '${MealID}' or isnull('${MealID}','') = '')
        GROUP  BY S_SE_SD_Setmealid,S_SE_SB_ShopCode,B_DP_DepartmentName,
                  S_SM_SM_Title
        UNION ALL
SELECT S_SE_SD_Setmealid               AS MealID,
               S_SM_SM_Title                   AS MealName,
               0                               AS num1,
				S_SE_SB_ShopCode as S_SE_SB_ShopCode,
				B_DP_DepartmentName as B_DP_DepartmentName,
               COUNT(DISTINCT S_SE_SD_SalesID) AS num2
        FROM   S_SE_SalesBasic
				INNER join B_Departments
				 ON B_DP_DepartmentID=S_SE_SB_ShopCode
               INNER JOIN S_SE_SalesDetail
                 ON S_SE_SB_SalesID = S_SE_SD_SalesID

               INNER JOIN S_SM_SetMealRecord
                 ON S_SE_SD_Setmealid = S_SM_SM_ID

       where S_SE_SB_ValueFlag='1' and S_SE_SB_WithdrawFlag='1'
	and convert(varchar(10),S_SE_SB_WithdrawDate,23)>='${beginDate}'
	and convert(varchar(10),S_SE_SB_WithdrawDate,23)<='${endDate}'
	and ((S_SE_SB_ShopCode in (select departmentID from #shopCode) and isnull('${departmentID}','')<>'') or isnull('${departmentID}','')='')
	and (S_SM_SM_Classify = '${ClassifyID}' or isnull('${ClassifyID}','') = '')
	and (S_SE_SD_Setmealid = '${MealID}' or isnull('${MealID}','') = '')
        GROUP  BY S_SE_SD_Setmealid,S_SE_SB_ShopCode,B_DP_DepartmentName,
                  S_SM_SM_Title
)t1 

GROUP  BY MealID,
       MealName,S_SE_SB_ShopCode,
       B_DP_DepartmentName


drop table #shopCode]]></Query>
</TableData>
