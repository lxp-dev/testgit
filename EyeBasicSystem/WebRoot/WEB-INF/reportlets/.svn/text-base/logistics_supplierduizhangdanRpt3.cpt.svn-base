<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select B_SP_ID,isnull((isnull(m.B_SP_StartPrice,0)+isnull(temp.contactamount,0)),0) as contactamount
  from B_Supplier 
    left join (
		select L_SC_SA_SupplierID as L_SC_SA_SupplierID,isnull((isnull(sum(isnull(L_SC_SA_CostPriceAmount,0)+isnull(L_SC_SA_PayMentAmount,0)+isnull(L_SC_SA_PrePayAmount,0)+isnull(L_SC_SA_SupplierSubAmount,0)+isnull(L_SC_SA_ManualAmount,0)),0)),0) as contactamount
		  from L_SC_SupplierAccount left join B_Supplier on L_SC_SA_SupplierID=B_SP_ID
		  where 1=1 and ((isnull('${supplierID}','')<>'' and L_SC_SA_SupplierID='${supplierID}') or (isnull('${supplierID}','')=''))
				and convert(varchar(10),L_SC_SA_Date,120)<'${bgnDate}'
		  group by L_SC_SA_SupplierID
    )temp on B_SP_ID=temp.L_SC_SA_SupplierID

    left join (
        select B_SP_LinkSupplierID,sum(B_SP_StartPrice) as B_SP_StartPrice
          from B_SupplierOpening inner join B_SupplierAgent on B_SP_SupplierID = B_SP_ID
          where B_SP_CompanyID = '${logincompanyid}'
                and ((isnull('${supplierID}','')<>'' and B_SP_LinkSupplierID='${supplierID}') or (isnull('${supplierID}','')=''))
          group by B_SP_LinkSupplierID

    )m  on B_SP_ID=m.B_SP_LinkSupplierID

  where 1=1 and ((isnull('${supplierID}','')<>'' and B_SP_ID='${supplierID}') or (isnull('${supplierID}','')=''))

]]></Query>
</TableData>
