</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select C_ST_I_BillID into #billinfo 
  from ( 
	select C_ST_I_BillID 
	  from C_ST_Inventory  
		   left join B_Warehouse on C_ST_I_InStockId = B_WH_ID  
		   left join B_Departments on B_WH_deptID = B_DP_DepartmentID  
	  where C_ST_I_BillTypeId in ('1','9') 
	        and ((isnull('${supplierID}','')<>'' and C_ST_I_SupplierId='${supplierID}' ) or (isnull('${supplierID}','')=''))
			and B_DP_CompanysID = '${logincompanyid}' 		
	union 
	select C_ST_I_BillID 
	  from C_ST_Inventory  
		   left join B_Warehouse on C_ST_I_OutStockId = B_WH_ID  
		   left join B_Departments on B_WH_deptID = B_DP_DepartmentID  
	  where C_ST_I_BillTypeId = '2' 
	        and ((isnull('${supplierID}','')<>'' and C_ST_I_SupplierId='${supplierID}' ) or (isnull('${supplierID}','')=''))
			and B_DP_CompanysID = '${logincompanyid}' 
	union 
	select L_PB_PB_ID 
	  from L_PB_PB_PaymentBill 
		   left join B_Departments on L_PB_PB_DepartmentID = B_DP_DepartmentID  
	  where B_DP_CompanysID = '${logincompanyid}' 
            and ((isnull('${supplierID}','')<>'' and L_PB_PB_SupplierID='${supplierID}' ) or (isnull('${supplierID}','')='')) 
  )m 

select B_SP_ID as supplierID,B_SP_SupplierName as supplierName,isnull(billamount,0) as billamount,
       isnull(receiptamount,0) as receiptamount,isnull(prePayAmount,0) as prePayAmount,
       isnull(supplierSubAmount,0) as supplierSubAmount,isnull(manualAmount,0) as manualAmount
  from B_Supplier left join (
	select isnull(sum(isnull(L_SC_SA_CostPriceAmount,0)),0) as billamount,
		   isnull(sum(isnull(L_SC_SA_PayMentAmount,0)),0) as receiptamount,
		   L_SC_SA_SupplierID as supplierID,
		   isnull(sum(isnull(L_SC_SA_PrePayAmount,0)),0) as prePayAmount,
		   isnull(sum(isnull(L_SC_SA_SupplierSubAmount,0)),0) as supplierSubAmount,
		   isnull(sum(isnull(L_SC_SA_ManualAmount,0)),0) as manualAmount
	  from L_SC_SupplierAccount
	  where L_SC_SA_BillID in (select C_ST_I_BillID from #billinfo)
			and ((isnull('${supplierID}','')<>'' and L_SC_SA_SupplierID='${supplierID}' ) or (isnull('${supplierID}','')=''))
			and ((isnull('${bgnDate}','')<>'' and  convert(varchar(10),L_SC_SA_Date,120)>='${bgnDate}') or (isnull('${bgnDate}','')=''))
			and convert(varchar(10),L_SC_SA_Date,120)<='${endDate}'
	  group by L_SC_SA_SupplierID
)t1 on B_SP_ID = supplierID
 where B_SP_ID <> 'ZZ'
       and ((isnull('${supplierID}','')<>'' and B_SP_ID='${supplierID}' ) or (isnull('${supplierID}','')=''))
order by B_SP_ID

drop table #billinfo

]]></Query>
</TableData>
