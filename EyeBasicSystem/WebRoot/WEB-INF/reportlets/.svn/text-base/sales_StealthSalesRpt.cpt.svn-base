<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select departmentID into #shopCode from sales_createShopCodeTab('${departmentsID}')
select 
R_SD_DSE_ShopCode,
R_SD_DSE_ShopName,
R_RC_DSE_SupplierID,
R_RC_DSE_SupplierName,
R_RC_DSE_BandID,
R_RC_DSE_BandName,
case 
when R_RC_DSE_tealthClass=1 then '日抛'
when R_RC_DSE_tealthClass=2 then '周抛'
when R_RC_DSE_tealthClass=3 then '月抛' 
when R_RC_DSE_tealthClass=4 then '季抛' 
when R_RC_DSE_tealthClass=5 then '半年抛'
when R_RC_DSE_tealthClass=6 then '年抛' 
when R_RC_DSE_tealthClass=7 then 'RGP' 
end as tealthClass,
R_RC_DSE_CostPrice,
R_RC_DSE_RetailPrice,
sum(R_SD_DSE_GoodsNum) as GoodsNum, 
sum(R_SD_DSE_GoodsRateAmount) as GoodsRateAmount,
sum(R_SD_DSE_GoodsPriceAmount) as GoodsPriceAmount,
sum(R_SD_DSE_GoodsAmount) as GoodsAmount
from R_RC_DaySalesEntry
where 1=1
and R_RC_DSE_GoodsCategoryID='4'
and ((isnull('${departmentsID}','')<>'' and R_SD_DSE_ShopCode in (select departmentID from #shopCode )) or (isnull('${departmentsID}','')=''))
AND CONVERT(VARCHAR(10), R_SD_DSE_Date, 23) >= '${begintime}'
AND CONVERT(VARCHAR(10), R_SD_DSE_Date, 23) <= '${endtime}'
and ((isnull('${supplierID}','')<>'' and R_RC_DSE_SupplierID = '${supplierID}') or (isnull('${supplierID}','')=''))
and ((isnull('${BandID}','')<>'' and R_RC_DSE_BandID = '${BandID}') or (isnull('${BandID}','')=''))
and ((isnull('${stealthclass}','')<>'' and R_RC_DSE_tealthClass = '${stealthclass}') or (isnull('${stealthclass}','')=''))
group by 
R_SD_DSE_ShopCode,
R_SD_DSE_ShopName,
R_RC_DSE_SupplierID,
R_RC_DSE_SupplierName,
R_RC_DSE_BandID,
R_RC_DSE_BandName,
R_RC_DSE_tealthClass,
R_RC_DSE_CostPrice,
R_RC_DSE_RetailPrice
order by R_RC_DSE_SupplierID,R_RC_DSE_BandID

drop table #shopCode]]></Query>
</TableData>
