<Query>
<![CDATA[select 
case
when S_SE_SB_RecipeType = '3' then   CONVERT (varchar(5), CONVERT (DECIMAL(5, 1),S_SE_SB_InterHighOD ) 
                                   + CONVERT (DECIMAL(5, 1), S_SE_SB_InterHighOS) ) 
when S_SE_SB_RecipeType = '1' then   CONVERT (varchar(5), CONVERT (DECIMAL(5, 1),S_SE_SB_InterHighOD ) 
                                   + CONVERT (DECIMAL(5, 1), S_SE_SB_InterHighOS) ) 


else ''
end as '远瞳距之和'
FROM   dbo.S_SE_SalesBasic
WHERE  S_SE_SB_SalesID = '${salesid}'
union all 
select 
case
when S_SE_SB_RecipeType = '3' then   CONVERT (varchar(5), CONVERT (DECIMAL(5, 1),S_SE_SB_InterHighOD ) 
                                   + CONVERT (DECIMAL(5, 1), S_SE_SB_InterHighOS) ) 
when S_SE_SB_RecipeType = '1' then   CONVERT (varchar(5), CONVERT (DECIMAL(5, 1),S_SE_SB_InterHighOD ) 
                                   + CONVERT (DECIMAL(5, 1), S_SE_SB_InterHighOS) ) 


else ''
end as '远瞳距之和'
FROM   dbo.S_SE_SalesBasic_Finished
WHERE  S_SE_SB_SalesID = '${salesid}'
]]></Query>
