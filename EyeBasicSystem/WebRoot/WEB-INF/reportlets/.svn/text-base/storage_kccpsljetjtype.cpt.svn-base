</Parameter>
<Parameter>
<Attributes name="stockId"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[DECLARE @brandNameStr VARCHAR(32)

SET @brandNameStr = '${brandNameStr}'

/* 添加 品种名称 查询 by ZK end*/
SELECT stockId
INTO   #stroageStockId
FROM   Storage_createstockidtab('${stockId}')

SELECT *
FROM   (SELECT tt.GoodsCategoryID    AS GoodsCategoryID,
               tt.B_DP_Type,
               tt.GoodsCategoryName  AS GoodsCategoryName,
               tt.StockId            AS StockId,
               tt.warehouseName      AS warehouseName,
               SUM(tt.GoodsQuantity) AS GoodsQuantity,
               SUM(tt.CostPrice)     AS CostPrice
        FROM   (SELECT t.GoodsId                             AS GoodsId,
                       B_DP_Type,
                       B_WH_warehouseName                    AS warehouseName,
                       B_SP_ID                       AS SupplierID,
                       B_SP_SupplierName                     AS SupplierName,
                       B_GC_ID                               AS GoodsCategoryID,
                       B_GC_GoodsCategoryName                AS GoodsCategoryName,
                       t.StockId                             AS StockId,
                       SUM(t.GoodsQuantity)                  AS GoodsQuantity,
                       SUM(t.CostPrice) AS CostPrice 
                FROM   (SELECT substring(C_SH_SB_GoodsId,1,4)       AS GoodsId,
                               C_SH_SB_StockId       AS StockId,
                               sum(C_SH_SB_GoodsQuantity) AS GoodsQuantity,
                               sum(C_SH_SB_GoodsQuantity*B_GI_CostPrice) AS CostPrice
                        FROM   C_SH_StorageBeginning inner join B_GoodsInfo on C_SH_SB_GoodsId=B_GI_GoodsID
                        group by substring(C_SH_SB_GoodsId,1,4),C_SH_SB_StockId
                        UNION ALL
                        SELECT substring(C_SH_SC_GoodsId,1,4)       AS GoodsId,
                               C_SH_SC_StockId       AS StockId,
                               sum(C_SH_SC_GoodsQuantity) AS GoodsQuantity,
                               sum(C_SH_SC_GoodsQuantity*B_GI_CostPrice) AS CostPrice
                        FROM   C_SH_StorageChange inner join B_GoodsInfo on C_SH_SC_GoodsId=B_GI_GoodsID
                        group by substring(C_SH_SC_GoodsId,1,4),C_SH_SC_StockId
                       )t
                       INNER JOIN B_GoodsCategory
                         ON substring(t.GoodsId,1,1) = B_GC_ID
                       INNER JOIN B_Supplier
                         ON B_SP_ID = substring(t.GoodsId,3,2)
                       INNER JOIN B_Warehouse
                         ON t.StockId = B_WH_ID
                       INNER JOIN B_Departments
                         ON B_WH_deptID = B_DP_DepartmentID
                WHERE  substring(t.GoodsId,3,2) <> 'ZZ'
                    and ((isnull('${goodsCategoryID}','')<>'' and substring(t.GoodsId,1,1)='${goodsCategoryID}' ) or (isnull('${goodsCategoryID}','')=''))
					and ((isnull('${stockId}','')<>'' and t.StockId in (select StockId from #stroageStockId)) or (isnull('${stockId}','')=''))
					and ((isnull('${supplierID}','')<>'' and substring(t.GoodsId,3,2)='${supplierID}') or (isnull('${supplierID}','')=''))

                GROUP  BY t.GoodsId,
                          B_WH_warehouseName,
                          B_SP_ID,
                          B_SP_SupplierName,
                          B_GC_ID,
                          B_GC_GoodsCategoryName,
                          B_DP_Type,
                          t.StockId)tt
        GROUP  BY tt.GoodsCategoryID,
                  tt.GoodsCategoryName,
                  tt.B_DP_Type,
                  tt.warehouseName,
                  tt.StockId)ttt
WHERE  ttt.GoodsQuantity <> 0
order by ttt.B_DP_Type desc

DROP TABLE #stroageStockId ]]></Query>
</TableData>
