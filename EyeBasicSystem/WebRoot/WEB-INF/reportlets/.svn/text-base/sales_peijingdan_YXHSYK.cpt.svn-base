<TableData name="ds5" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="salesid"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
SELECT S_SE_SD_SalesItemID AS '商品代码',
CASE
  WHEN B_GI_GoodsCategoryID = '1' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_Color), '')
  WHEN B_GI_GoodsCategoryID = '6' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_Color), '')
  WHEN B_GI_GoodsCategoryID = '8' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_SupplierColor), '') 

 ELSE Isnull(B_GI_GoodsName, '')
END                                         AS '商品名称',
       CASE
         WHEN S_SE_SD_CommoditiesFlag = '3' THEN B_GI_Spec + '-' + B_GI_Color + '-' + B_GI_FrameSize
         WHEN S_SE_SD_CommoditiesFlag = '6' THEN B_GI_Spec + '-' + B_GI_Color + '-' + B_GI_FrameSize
         ELSE B_GI_Spec
       END                 AS '商品规格',
       CASE
         WHEN S_SE_SD_CommoditiesFlag = '2' THEN '99'
         ELSE S_SE_SD_CommoditiesFlag
       END                 AS '排序',
       B_UT_unitName       AS '单位名称',
       S_SE_SD_Number      AS '商品数量',
[S_SE_SD_SPrice]A AS '商品单价',
       S_SE_SD_SalesValue  AS '应收金额'

FROM   F_SystemParameter,S_SE_SalesDetail
       INNER JOIN S_SE_SalesBasic
         ON S_SE_SB_SalesID = S_SE_SD_SalesID
       LEFT JOIN B_GoodsInfo
         ON B_GI_GoodsID = S_SE_SD_SalesItemID
       LEFT JOIN B_Unit
         ON B_GI_UnitId = B_UT_id
	   LEFT JOIN S_SM_SetMeal
		 ON S_SM_SM_ID = S_SE_SD_Setmealid
WHERE  S_SE_SD_SalesID = 
'${salesid}'

union all 

SELECT S_SE_SD_SalesItemID AS '商品代码',
CASE
  WHEN B_GI_GoodsCategoryID = '1' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_Color), '')
  WHEN B_GI_GoodsCategoryID = '6' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_Color), '')
  WHEN B_GI_GoodsCategoryID = '8' THEN Isnull(B_GI_GoodsName, '') + Isnull('  —' + Rtrim(B_GI_Spec), '') + Isnull('—' + Rtrim(B_GI_SupplierColor), '') 

 ELSE Isnull(B_GI_GoodsName, '')
END                                         AS '商品名称',
       CASE
         WHEN S_SE_SD_CommoditiesFlag = '3' THEN B_GI_Spec + '-' + B_GI_Color + '-' + B_GI_FrameSize
         WHEN S_SE_SD_CommoditiesFlag = '6' THEN B_GI_Spec + '-' + B_GI_Color + '-' + B_GI_FrameSize
         ELSE B_GI_Spec
       END                 AS '商品规格',
       CASE
         WHEN S_SE_SD_CommoditiesFlag = '2' THEN '99'
         ELSE S_SE_SD_CommoditiesFlag
       END                 AS '排序',
       B_UT_unitName       AS '单位名称',
       S_SE_SD_Number      AS '商品数量',
[S_SE_SD_SPrice]A AS '商品单价',
       S_SE_SD_SalesValue  AS '应收金额'

FROM   F_SystemParameter,S_SE_SalesDetail_Finished
       INNER JOIN S_SE_SalesBasic_Finished
         ON S_SE_SB_SalesID = S_SE_SD_SalesID
       LEFT JOIN B_GoodsInfo
         ON B_GI_GoodsID = S_SE_SD_SalesItemID
       LEFT JOIN B_Unit
         ON B_GI_UnitId = B_UT_id
	   LEFT JOIN S_SM_SetMeal
		 ON S_SM_SM_ID = S_SE_SD_Setmealid
WHERE  S_SE_SD_SalesID = 
'${salesid}'
ORDER BY [排序]A]]></Query>
