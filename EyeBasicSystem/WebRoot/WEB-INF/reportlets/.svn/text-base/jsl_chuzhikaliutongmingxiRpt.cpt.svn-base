<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[declare @cardID varchar(50)
set @cardID='${cardID}'

select C_ST_CZK_ID as cardid,convert(varchar(16),C_ST_CZK_Jiankariqi,120) as carddate,
       isnull(B_DP_DepartmentName,'') as dptname,convert(varchar(16),C_ST_SalesDate,120) as salesdate,
       isnull(C_ST_SalesValue,0) as salesamount,isnull(C_ST_CZK_Jine,0) as notsalesamount
    from C_ST_Chuzhika left join B_Departments on C_ST_SalesStore=B_DP_DepartmentID
    where isnull(C_ST_CZK_Jine,0)>0 and ((isnull(@cardID,'')<>'' and C_ST_CZK_ID like '%' + @cardID + '%') or isnull(@cardID,'')='')

]]></Query>
</TableData>
