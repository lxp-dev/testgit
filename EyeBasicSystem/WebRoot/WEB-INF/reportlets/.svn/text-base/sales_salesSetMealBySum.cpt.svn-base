<Query>
<![CDATA[SELECT departmentID
INTO   #shopCode
FROM   Sales_createshopcodetab('${shopCode}')

SELECT '${bgnDate}' + '～' + '${endDate}'             AS printdate,
       --temp.SalesItemID as GoodsID,
       --B_GI_GoodsName as GoodsName,
       B_GC_ID + B_BD_ID + B_SP_ID                   AS GoodsID,
       B_BD_ID                                       AS brandID,
       B_BD_brandName                                AS brandName,
       B_SP_ID                                       AS SupplierID,
       B_SP_SupplierName                             AS SupplierName,
       B_GC_ID                                       AS CategoryID,
       B_GC_GoodsCategoryName                        AS CategoryName,
       SUM(TEMP.num1)                                AS num1,
       SUM(TEMP.salesValue1)                         AS salesValue1,
       SUM(TEMP.num2)                                AS num2,
       SUM(TEMP.salesValue2)                         AS salesValue2,
       SUM(TEMP.num1) + SUM(TEMP.num2)               AS num3,
       SUM(TEMP.salesValue1) + SUM(TEMP.salesValue2) AS salesValue3
FROM   (SELECT tt.SalesItemID               AS SalesItemID,
               Isnull(t1.num1, 0)           AS num1,
               Isnull(t1.salesValue1, 0.00) AS salesValue1,
               Isnull(t2.num2, 0)           AS num2,
               Isnull(t2.salesValue2, 0.00) AS salesValue2
        FROM  (SELECT SalesItemID
               FROM   (SELECT S_SE_SD_SalesItemID AS SalesItemID
                       FROM   S_SE_SalesDetail
                              INNER JOIN S_SE_SalesBasic
                                ON S_SE_SD_SalesID = S_SE_SB_SalesID
                              INNER JOIN S_SM_SetMealRecord
                                ON S_SM_SM_ID = S_SE_SD_Setmealid
                       WHERE  1 = 1
                              AND ( ( Isnull('${ClassifyID}', '') = ''
                                       OR Isnull('${ClassifyID}', '') = '1'
                                          AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '2'
                                              AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '3'
                                              AND S_SE_SB_OrdersType = '5' ) )
                              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) >= '${bgnDate}'
                              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) <= '${endDate}'
                              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                           FROM   #shopCode)
                                      AND Isnull('${shopCode}', '') <> '' )
                                     OR Isnull('${shopCode}', '') = '' )
                              AND ( S_SM_SM_ID = '${MealID}'
                                     OR Isnull('${MealID}', '') = '' )
                              AND S_SE_SB_ValueFlag = '1'
                       UNION ALL
                       SELECT S_SE_SD_SalesItemID AS SalesItemID
                       FROM   S_SE_SalesDetail
                              INNER JOIN S_SE_SalesBasic
                                ON S_SE_SD_SalesID = S_SE_SB_SalesID
                              INNER JOIN S_SM_SetMealRecord
                                ON S_SM_SM_ID = S_SE_SD_Setmealid
                       WHERE  1 = 1
                              AND ( ( Isnull('${ClassifyID}', '') = ''
                                       OR Isnull('${ClassifyID}', '') = '1'
                                          AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '2'
                                              AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '3'
                                              AND S_SE_SB_OrdersType = '5' ) )
                              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) >= '${bgnDate}'
                              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) <= '${endDate}'
                              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                           FROM   #shopCode)
                                      AND Isnull('${shopCode}', '') <> '' )
                                     OR Isnull('${shopCode}', '') = '' )
                              AND ( S_SM_SM_ID = '${MealID}'
                                     OR Isnull('${MealID}', '') = '' )
                              AND S_SE_SB_ValueFlag = '1'
                              AND S_SE_SB_WithdrawFlag = '1'
					   union all
					   SELECT S_SE_SD_SalesItemID AS SalesItemID
                       FROM   S_SE_SalesDetail_Finished
                              INNER JOIN S_SE_SalesBasic_Finished
                                ON S_SE_SD_SalesID = S_SE_SB_SalesID
                              INNER JOIN S_SM_SetMealRecord
                                ON S_SM_SM_ID = S_SE_SD_Setmealid
                       WHERE  1 = 1
                              AND ( ( Isnull('${ClassifyID}', '') = ''
                                       OR Isnull('${ClassifyID}', '') = '1'
                                          AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '2'
                                              AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '3'
                                              AND S_SE_SB_OrdersType = '5' ) )
                              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) >= '${bgnDate}'
                              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) <= '${endDate}'
                              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                           FROM   #shopCode)
                                      AND Isnull('${shopCode}', '') <> '' )
                                     OR Isnull('${shopCode}', '') = '' )
                              AND ( S_SM_SM_ID = '${MealID}'
                                     OR Isnull('${MealID}', '') = '' )
                              AND S_SE_SB_ValueFlag = '1'
                       UNION ALL
                       SELECT S_SE_SD_SalesItemID AS SalesItemID
                       FROM   S_SE_SalesDetail_Finished
                              INNER JOIN S_SE_SalesBasic_Finished
                                ON S_SE_SD_SalesID = S_SE_SB_SalesID
                              INNER JOIN S_SM_SetMealRecord
                                ON S_SM_SM_ID = S_SE_SD_Setmealid
                       WHERE  1 = 1
                              AND ( ( Isnull('${ClassifyID}', '') = ''
                                       OR Isnull('${ClassifyID}', '') = '1'
                                          AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '2'
                                              AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                     OR ( Isnull('${ClassifyID}', '') = ''
                                           OR Isnull('${ClassifyID}', '') = '3'
                                              AND S_SE_SB_OrdersType = '5' ) )
                              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) >= '${bgnDate}'
                              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) <= '${endDate}'
                              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                           FROM   #shopCode)
                                      AND Isnull('${shopCode}', '') <> '' )
                                     OR Isnull('${shopCode}', '') = '' )
                              AND ( S_SM_SM_ID = '${MealID}'
                                     OR Isnull('${MealID}', '') = '' )
                              AND S_SE_SB_ValueFlag = '1'
                              AND S_SE_SB_WithdrawFlag = '1')t
               GROUP  BY SalesItemID)tt
              LEFT JOIN (SELECT S_SE_SD_SalesItemID     AS SalesItemID,
                                SUM(S_SE_SD_Number)     AS num1,
                                SUM(S_SE_SD_SalesValue) AS salesValue1
                         FROM   S_SE_SalesDetail
                                INNER JOIN S_SE_SalesBasic
                                  ON S_SE_SD_SalesID = S_SE_SB_SalesID
                                INNER JOIN S_SM_SetMealRecord
                                  ON S_SM_SM_ID = S_SE_SD_Setmealid
                         WHERE  1 = 1
                                AND ( ( Isnull('${ClassifyID}', '') = ''
                                         OR Isnull('${ClassifyID}', '') = '1'
                                            AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '2'
                                                AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '3'
                                                AND S_SE_SB_OrdersType = '5' ) )
                                AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) >= '${bgnDate}'
                                AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) <= '${endDate}'
                                AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                             FROM   #shopCode)
                                        AND Isnull('${shopCode}', '') <> '' )
                                       OR Isnull('${shopCode}', '') = '' )
                                AND ( S_SM_SM_ID = '${MealID}'
                                       OR Isnull('${MealID}', '') = '' )
                                AND S_SE_SB_ValueFlag = '1'
                         GROUP  BY S_SE_SD_SalesItemID
                         union all
                         SELECT S_SE_SD_SalesItemID     AS SalesItemID,
                                SUM(S_SE_SD_Number)     AS num1,
                                SUM(S_SE_SD_SalesValue) AS salesValue1
                         FROM   S_SE_SalesDetail_Finished
                                INNER JOIN S_SE_SalesBasic_Finished
                                  ON S_SE_SD_SalesID = S_SE_SB_SalesID
                                INNER JOIN S_SM_SetMealRecord
                                  ON S_SM_SM_ID = S_SE_SD_Setmealid
                         WHERE  1 = 1
                                AND ( ( Isnull('${ClassifyID}', '') = ''
                                         OR Isnull('${ClassifyID}', '') = '1'
                                            AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '2'
                                                AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '3'
                                                AND S_SE_SB_OrdersType = '5' ) )
                                AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) >= '${bgnDate}'
                                AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) <= '${endDate}'
                                AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                             FROM   #shopCode)
                                        AND Isnull('${shopCode}', '') <> '' )
                                       OR Isnull('${shopCode}', '') = '' )
                                AND ( S_SM_SM_ID = '${MealID}'
                                       OR Isnull('${MealID}', '') = '' )
                                AND S_SE_SB_ValueFlag = '1'
                         GROUP  BY S_SE_SD_SalesItemID)t1
                ON tt.SalesItemID = t1.SalesItemID
              LEFT JOIN (SELECT S_SE_SD_SalesItemID      AS SalesItemID,
                                -SUM(S_SE_SD_Number)     AS num2,
                                -SUM(S_SE_SD_SalesValue) AS salesValue2
                         FROM   S_SE_SalesDetail
                                INNER JOIN S_SE_SalesBasic
                                  ON S_SE_SD_SalesID = S_SE_SB_SalesID
                                INNER JOIN S_SM_SetMealRecord
                                  ON S_SM_SM_ID = S_SE_SD_Setmealid
                         WHERE  1 = 1
                                AND ( ( Isnull('${ClassifyID}', '') = ''
                                         OR Isnull('${ClassifyID}', '') = '1'
                                            AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '2'
                                                AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '3'
                                                AND S_SE_SB_OrdersType = '5' ) )
                                AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) >= '${bgnDate}'
                                AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) <= '${endDate}'
                                AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                             FROM   #shopCode)
                                        AND Isnull('${shopCode}', '') <> '' )
                                       OR Isnull('${shopCode}', '') = '' )
                                AND ( S_SM_SM_ID = '${MealID}'
                                       OR Isnull('${MealID}', '') = '' )
                                AND S_SE_SB_ValueFlag = '1'
                                AND S_SE_SB_WithdrawFlag = '1'
                         GROUP  BY S_SE_SD_SalesItemID
                         union all
                         SELECT S_SE_SD_SalesItemID      AS SalesItemID,
                                -SUM(S_SE_SD_Number)     AS num2,
                                -SUM(S_SE_SD_SalesValue) AS salesValue2
                         FROM   S_SE_SalesDetail_Finished
                                INNER JOIN S_SE_SalesBasic_Finished
                                  ON S_SE_SD_SalesID = S_SE_SB_SalesID
                                INNER JOIN S_SM_SetMealRecord
                                  ON S_SM_SM_ID = S_SE_SD_Setmealid
                         WHERE  1 = 1
                                AND ( ( Isnull('${ClassifyID}', '') = ''
                                         OR Isnull('${ClassifyID}', '') = '1'
                                            AND S_SE_SB_OrdersType IN ( '1', '2' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '2'
                                                AND S_SE_SB_OrdersType IN ( '3', '4' ) )
                                       OR ( Isnull('${ClassifyID}', '') = ''
                                             OR Isnull('${ClassifyID}', '') = '3'
                                                AND S_SE_SB_OrdersType = '5' ) )
                                AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) >= '${bgnDate}'
                                AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) <= '${endDate}'
                                AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                                             FROM   #shopCode)
                                        AND Isnull('${shopCode}', '') <> '' )
                                       OR Isnull('${shopCode}', '') = '' )
                                AND ( S_SM_SM_ID = '${MealID}'
                                       OR Isnull('${MealID}', '') = '' )
                                AND S_SE_SB_ValueFlag = '1'
                                AND S_SE_SB_WithdrawFlag = '1'
                         GROUP  BY S_SE_SD_SalesItemID)t2
                ON tt.SalesItemID = t2.SalesItemID)TEMP
       INNER JOIN B_GoodsInfo
         ON SalesItemID = B_GI_GoodsID
       INNER JOIN B_Supplier
         ON B_GI_SupplierID = B_SP_ID
       INNER JOIN B_Brand
         ON B_GI_BrandID = B_BD_ID
            AND B_GI_SupplierID = B_BD_SupplierID
       INNER JOIN B_GoodsCategory
         ON B_GC_ID = B_GI_GoodsCategoryID
GROUP  BY B_BD_ID,
          B_BD_brandName,
          B_SP_ID,
          B_SP_SupplierName,
          B_GC_ID,
          B_GC_GoodsCategoryName,
          B_GC_ID + B_BD_ID + B_SP_ID
order by B_GC_ID
DROP TABLE #shopCode ]]></Query>
