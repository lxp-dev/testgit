<Query>
<![CDATA[SELECT C_SHA_AA_inDepartmentId AS departmentid,
       B_DP_DepartmentName     AS departmentname
FROM   C_SHA_ApplyAllocation
       INNER JOIN B_Departments
         ON C_SHA_AA_inDepartmentId = B_DP_DepartmentID
WHERE  C_SHA_AA_billID = (SELECT C_ST_P_ApplyBillID
                          FROM   C_ST_Po
                          WHERE  C_ST_P_ID = '${poID}') ]]></Query>
