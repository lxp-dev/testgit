<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select 
B_GC_GoodsCategoryName,
B_GC_ID,
B_GI_GoodsName,
B_GI_Spec,
isnull(B_GI_SupplierColor,'') as SupplierColor ,
B_GI_Sph,
B_GI_Cyl,
B_UT_unitName,
C_ST_PE_OrderNumber,
C_ST_PE_Remark,
B_GI_CostPrice
 from C_ST_PoEntry
inner join B_GoodsInfo on B_GI_GoodsID=C_ST_PE_goodsID
inner join B_Unit on B_GI_UnitId=B_UT_id
inner join B_GoodsCategory on B_GC_ID=B_GI_GoodsCategoryID
where C_ST_PE_PurchaseOrderID='${poID}']]></Query>
</TableData>
