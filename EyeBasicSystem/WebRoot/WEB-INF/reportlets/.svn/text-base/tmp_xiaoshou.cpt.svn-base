<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT DISTINCT SYS_PersonInfo.ID                AS mslpersonid,
                personName                       AS personName,
                departmentname=Stuff((SELECT ',' + B_DP_DepartmentName
                                      FROM   SYS_PersonDepartments,
                                             B_Departments
                                      WHERE  SYS_PersonInfo.ID = SYS_PD_PersonID
                                             AND SYS_PD_DepartmentID = B_DP_DepartmentID
                                      FOR XML PATH('')), 1, 1, ''),
                CASE sex
                  WHEN '1' THEN '男'
                  WHEN '2' THEN '女'
                END                              AS sex,

                Isnull(M_SL_sumsalary, '0')      AS mslsumsalary,
                Isnull(M_SL_baoxian, '0')      AS baoxian,
                Isnull(M_SL_gongjijin, '0')      AS gongjijin,
				isnull(xiaoshou,'0') as xiaoshou
FROM   SYS_PersonInfo
       LEFT JOIN M_Salary
         ON M_Salary.M_SL_PersonId = SYS_PersonInfo.id
       LEFT JOIN SYS_PersonRoles
         ON SYS_PersonRoles.personID = SYS_PersonInfo.id
       LEFT JOIN SYS_Roles
         ON SYS_Roles.roleid = SYS_PersonRoles.roleid
       LEFT JOIN tmp_xiaoshou
         ON tmp_xiaoshou.personid = SYS_PersonInfo.id
	
WHERE  1 = 1
${if(len(selId)==0,""," and SYS_PersonInfo.id='"+selId+"'") }
${if(len(selPersonName)==0,""," and personName='"+selPersonName+"'") }
${if(len(selyear)==0,""," and M_SL_Year='"+selyear+"'") }
${if(len(selmonth)==0,""," and M_SL_Month='"+selmonth+"'") }
${if(len(selDepartmentID)==0,""," and sys_personinfo.id in(select SYS_PD_PersonID from SYS_PersonDepartments where sys_pd_departmentid='"+selDepartmentID+"')")}
${if(len(isinvocation)==0,""," and isInvocation='"+isinvocation+"'") }
${if(len(selRoleid)==0,""," and SYS_PersonRoles.roleID='"+selRoleid+"'") }
${if(len(begintime)==0,"","and convert(varchar(10),entrytime, 23) >='"+begintime+"'") }
${if(len(endtime)==0,"","and convert(varchar(10),entrytime, 23) <='"+endtime+"'") }
ORDER  BY departmentname ]]></Query>
</TableData>
