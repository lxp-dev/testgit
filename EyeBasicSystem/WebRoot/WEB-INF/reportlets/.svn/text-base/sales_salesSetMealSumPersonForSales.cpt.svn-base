<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="Meal"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="endDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="beginDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="departmentID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT departmentID
INTO   #shopCode
FROM   Sales_createshopcodetab('${departmentID}')

SELECT '${beginDate}' + '～' + '${endDate}' AS printdate,
       TEMP.SalesItemID                    AS SalesItemID,
       TEMP.SalesID                        AS SalesID,
       B_GI_GoodsName                      AS GoodsName,
       SUM(TEMP.num)                       AS num,
       SUM(TEMP.salesValue)                AS salesValue
FROM  (SELECT S_SE_SD_SalesItemID     AS SalesItemID,
              S_SE_SB_SalesID         AS SalesID,
              SUM(S_SE_SD_Number)     AS num,
              SUM(S_SE_SD_SalesValue) AS salesValue
       FROM   S_SE_SalesDetail
              INNER JOIN S_SE_SalesBasic
                ON S_SE_SD_SalesID = S_SE_SB_SalesID
              INNER JOIN S_SM_SetMealRecord
                ON S_SM_SM_ID = S_SE_SD_Setmealid
       WHERE  1 = 1
              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) >= '${beginDate}'
              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) <= '${endDate}'
              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                           FROM   #shopCode)
                      AND Isnull('${departmentID}', '') <> '' )
                     OR Isnull('${departmentID}', '') = '' )
              AND ( S_SM_SM_ID = '${Meal}'
                     OR Isnull('${Meal}', '') = '' )
              AND S_SE_SB_ValueFlag = '1'
       GROUP  BY S_SE_SD_SalesItemID,
                 S_SE_SB_SalesID
       UNION ALL
       SELECT S_SE_SD_SalesItemID      AS SalesItemID,
              S_SE_SB_SalesID          AS SalesID,
              -SUM(S_SE_SD_Number)     AS num,
              -SUM(S_SE_SD_SalesValue) AS salesValue
       FROM   S_SE_SalesDetail
              INNER JOIN S_SE_SalesBasic
                ON S_SE_SD_SalesID = S_SE_SB_SalesID
              INNER JOIN S_SM_SetMealRecord
                ON S_SM_SM_ID = S_SE_SD_Setmealid
       WHERE  1 = 1
              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) >= '${beginDate}'
              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) <= '${endDate}'
              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                           FROM   #shopCode)
                      AND Isnull('${departmentID}', '') <> '' )
                     OR Isnull('${departmentID}', '') = '' )
              AND ( S_SM_SM_ID = '${Meal}'
                     OR Isnull('${Meal}', '') = '' )
              AND S_SE_SB_ValueFlag = '1'
              AND S_SE_SB_WithdrawFlag = '1'
       GROUP  BY S_SE_SD_SalesItemID,
                 S_SE_SB_SalesID
       union all
       SELECT S_SE_SD_SalesItemID     AS SalesItemID,
              S_SE_SB_SalesID         AS SalesID,
              SUM(S_SE_SD_Number)     AS num,
              SUM(S_SE_SD_SalesValue) AS salesValue
       FROM   S_SE_SalesDetail_Finished
              INNER JOIN S_SE_SalesBasic_Finished
                ON S_SE_SD_SalesID = S_SE_SB_SalesID
              INNER JOIN S_SM_SetMealRecord
                ON S_SM_SM_ID = S_SE_SD_Setmealid
       WHERE  1 = 1
              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) >= '${beginDate}'
              AND CONVERT(VARCHAR(10), S_SE_SB_PosDatetime, 23) <= '${endDate}'
              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                           FROM   #shopCode)
                      AND Isnull('${departmentID}', '') <> '' )
                     OR Isnull('${departmentID}', '') = '' )
              AND ( S_SM_SM_ID = '${Meal}'
                     OR Isnull('${Meal}', '') = '' )
              AND S_SE_SB_ValueFlag = '1'
       GROUP  BY S_SE_SD_SalesItemID,
                 S_SE_SB_SalesID
       UNION ALL
       SELECT S_SE_SD_SalesItemID      AS SalesItemID,
              S_SE_SB_SalesID          AS SalesID,
              -SUM(S_SE_SD_Number)     AS num,
              -SUM(S_SE_SD_SalesValue) AS salesValue
       FROM   S_SE_SalesDetail_Finished
              INNER JOIN S_SE_SalesBasic_Finished
                ON S_SE_SD_SalesID = S_SE_SB_SalesID
              INNER JOIN S_SM_SetMealRecord
                ON S_SM_SM_ID = S_SE_SD_Setmealid
       WHERE  1 = 1
              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) >= '${beginDate}'
              AND CONVERT(VARCHAR(10), S_SE_SB_WithdrawDate, 23) <= '${endDate}'
              AND ( ( S_SE_SB_ShopCode IN (SELECT departmentID
                                           FROM   #shopCode)
                      AND Isnull('${departmentID}', '') <> '' )
                     OR Isnull('${departmentID}', '') = '' )
              AND ( S_SM_SM_ID = '${Meal}'
                     OR Isnull('${Meal}', '') = '' )
              AND S_SE_SB_ValueFlag = '1'
              AND S_SE_SB_WithdrawFlag = '1'
       GROUP  BY S_SE_SD_SalesItemID,
                 S_SE_SB_SalesID)TEMP
      INNER JOIN B_GoodsInfo
        ON TEMP.SalesItemID = B_GI_GoodsID
GROUP  BY TEMP.SalesItemID,
          TEMP.SalesID,
          B_GI_GoodsName
ORDER  BY num DESC,
          SalesID

DROP TABLE #shopCode ]]></Query>
</TableData>
