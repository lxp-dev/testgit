</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select departmentID into #shopCode from sales_createShopCodeTab('${shopcode}')
select 
'${begindate}' +'～'+ '${enddate}' as printdate,
S_SE_SB_SalesID as SalesID,
S_SE_SD_SalesItemID as SalesItemID,
B_GI_GoodsName as GoodsName,
B_DP_DepartmentName as  DepartmentName ,
convert(varchar(16),S_SE_SB_PosDatetime,120) as saleDate,
S_SE_SD_Number as Number,
B_GI_RetailPrice as RetailPrice,
B_GI_CostPrice as CostPrice
 from S_SE_SalesDetail
inner join S_SE_SalesBasic on S_SE_SB_SalesID=S_SE_SD_SalesID
 inner join b_goodsinfo on B_GI_GoodsID = S_SE_SD_SalesItemID
 inner join B_Departments on S_SE_SB_ShopCode=B_DP_DepartmentID
where 1=1
and (S_SE_SD_SalesItemID like '%'+'${goodsid}'+'%' or isnull('${goodsid}','') = '')
and convert(varchar(10),S_SE_SB_PosDatetime,120)>='${beginDate}'
and convert(varchar(10),S_SE_SB_PosDatetime,120)<='${endDate}'
 and B_GI_SupplierID <>'ZZ'  -- 去掉顾客自片/单眼片
 and S_SE_SB_ValueFlag='1'
union all
select 
'${begindate}' +'～'+ '${enddate}' as printdate,
S_SE_SB_SalesID as SalesID,
S_SE_SD_SalesItemID as SalesItemID,
B_GI_GoodsName as GoodsName,
B_DP_DepartmentName as  DepartmentName ,
convert(varchar(16),S_SE_SB_PosDatetime,120) as saleDate,
-S_SE_SD_Number as Number,
B_GI_RetailPrice as RetailPrice,
-B_GI_CostPrice as CostPrice
 from S_SE_SalesDetail
inner join S_SE_SalesBasic on S_SE_SB_SalesID=S_SE_SD_SalesID
 inner join b_goodsinfo on B_GI_GoodsID = S_SE_SD_SalesItemID
 inner join B_Departments on S_SE_SB_ShopCode=B_DP_DepartmentID
where 1=1
and (S_SE_SD_SalesItemID like '%'+'${goodsid}'+'%' or isnull('${goodsid}','') = '')
and ((S_SE_SB_ShopCode in (select departmentID from #shopCode) and isnull('${shopcode}','')<>'') or isnull('${shopcode}','')='')
and convert(varchar(10),S_SE_SB_WithdrawDate,120)>='${beginDate}'
and convert(varchar(10),S_SE_SB_WithdrawDate,120)<='${endDate}'
 and B_GI_SupplierID <>'ZZ'  -- 去掉顾客自片/单眼片
 and S_SE_SB_ValueFlag='1'
 and S_SE_SB_WithdrawFlag='1'-- 退款表示1 
drop table #shopCode
 ]]></Query>
</TableData>
