<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="bgnDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="billID"/>
<O>
<![CDATA[]]></O>
</Parameter>
<Parameter>
<Attributes name="endDate"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select substring(L_IE_RE_GoodsID,1,7) as goodsId,B_BD_brandName as goodsName,
       sum(L_IE_RE_CheckGoodsQuantity) as num,sum(L_IE_RE_NotTaxRateAmount) as notTaxRateAmount,
       sum(L_IE_RE_TaxAmount) as taxAmount,sum(L_IE_RE_CostPriceAmount) as costPriceAmount,
       (case substring('${billID}',2,1) 
         when 'O' then b.B_WH_warehouseName else a.B_WH_warehouseName end ) as warehouseName  
  from L_RE_ReversalEntry 
       inner join L_R_Reversal on L_I_R_ID=L_IE_RE_InvoiceID
       left join B_GoodsInfo on L_IE_RE_GoodsID=B_GI_GoodsID
       left join B_Brand on B_BD_ID=substring(L_IE_RE_GoodsID,6,2) and B_BD_SupplierID=substring(L_IE_RE_GoodsID,3,2)
       left join C_ST_Inventory on L_IE_RE_BillID=C_ST_I_BillID
       left join B_Warehouse a on C_ST_I_InStockId=a.B_WH_ID
       left join B_Warehouse b on C_ST_I_OutStockId=b.B_WH_ID
  where L_IE_RE_BillID='${billID}'
        and convert(varchar(10),L_I_R_Date,120)>='${bgnDate}'
        and convert(varchar(10),L_I_R_Date,120)<='${endDate}'
  group by substring(L_IE_RE_GoodsID,1,7),B_BD_brandName,a.B_WH_warehouseName,b.B_WH_warehouseName]]></Query>
</TableData>
