<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select C_ST_I_BillID as billID,convert(varchar(10),C_ST_I_billDate,120) as billDate,B_SP_SupplierName as supplierName,
       C_ST_I_SourceBillId as sourceBillId,c.personName as createPerson,a.personName as AuditPerson,
       convert(varchar(10),C_ST_I_AuditDate,120) as auditDate,C_ST_I_Remark as remark,
       (case C_ST_I_BillTypeId 
          when 9 then '委外收货单'
          when 1 then '采购收货单' else '采购退货单' end
       ) as billType
  from C_ST_Inventory left join B_Supplier on B_SP_ID=C_ST_I_SupplierId
       left join SYS_PersonInfo c on C_ST_I_createPerson=c.id
       left join SYS_PersonInfo a on C_ST_I_AuditPerson=a.id
  where C_ST_I_BillID='${billID}']]></Query>
</TableData>
