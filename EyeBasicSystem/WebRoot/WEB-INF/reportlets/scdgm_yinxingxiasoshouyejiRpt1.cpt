<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[
declare @bgnDate varchar(10),@endDate varchar(10),@shopCode varchar(100)

set @bgnDate = '${bgnDate}'
set @endDate = '${endDate}'
set @shopCode = '${shopCode}'

select * from (
select salerName as salerName,(isnull(bslsalesValue,0)+isnull(fbslsalesValue,0)) as salesValue,
       bslsalesValue as bslsalesValue,fbslsalesValue as fbslsalesValue,
       cast((bslsalesValue * 0.7) as numeric(30,2)) as bslsalesValue7,(bslsalesValue-cast((bslsalesValue * 0.7) as numeric(30,2))) as bslsalesValue3,
       cast((fbslsalesValue * 0.2) as numeric(30,2)) as fbslsalesValue2,(fbslsalesValue-cast((fbslsalesValue * 0.2) as numeric(30,2))) as fbslsalesValue8
  from ( 
select salerName as salerName,sum(isnull(bslsalesValue,0)) as bslsalesValue,sum(isnull(fbslsalesValue,0)) as fbslsalesValue from ( 
select salerName as salerName,sum(isnull(salesValue,0)) as bslsalesValue,0 as fbslsalesValue from ( 
select personName as salerName,sum(isnull(S_SE_SD_SalesValue,0)) as salesValue
  from S_SE_SalesBasic inner join S_SE_SalesDetail on S_SE_SB_SalesID=S_SE_SD_SalesID
       left join SYS_PersonInfo on S_SE_SB_SalerID=id
       left join B_GoodsInfo on S_SE_SD_SalesItemID=B_GI_GoodsID
  where B_GI_GoodsCategoryID in ('4','5') and B_GI_SupplierID='A3' and S_SE_SB_ValueFlag='1'
        and S_SE_SB_ShopCode=@shopCode
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SB_PosDatetime,120) >= @bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SB_PosDatetime,120) <= @endDate) or isnull(@endDate,'')='')
  group by personName
union all
select personName as salerName ,-sum(isnull(S_SE_SD_SalesValue,0)) as salesValue
  from S_SE_SalesBasic inner join S_SE_SalesDetail on S_SE_SB_SalesID=S_SE_SD_SalesID
       left join SYS_PersonInfo on S_SE_SB_SalerID=id
       left join B_GoodsInfo on S_SE_SD_SalesItemID=B_GI_GoodsID
  where B_GI_GoodsCategoryID in ('4','5') and B_GI_SupplierID='A3' and S_SE_SB_ValueFlag='1' and S_SE_SB_WithdrawFlag='1'
        and S_SE_SB_ShopCode=@shopCode
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SB_WithdrawDate,120) >= @bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SB_WithdrawDate,120) <= @endDate) or isnull(@endDate,'')='')
  group by personName
)temp
  group by salerName
---------------------------------------------------------------------------------------------
union all
select salerName as salerName,0 as bslsalesValue,sum(isnull(salesValue,0)) as fbslsalesValue from ( 
select personName as salerName,sum(isnull(S_SE_SD_SalesValue,0)) as salesValue
  from S_SE_SalesBasic inner join S_SE_SalesDetail on S_SE_SB_SalesID=S_SE_SD_SalesID
       left join SYS_PersonInfo on S_SE_SB_SalerID=id
       left join B_GoodsInfo on S_SE_SD_SalesItemID=B_GI_GoodsID
  where B_GI_GoodsCategoryID in ('4','5') and B_GI_SupplierID<>'A3' and S_SE_SB_ValueFlag='1'
        and S_SE_SB_ShopCode=@shopCode
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SB_PosDatetime,120) >= @bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SB_PosDatetime,120) <= @endDate) or isnull(@endDate,'')='')
  group by personName
union all
select personName as salerName ,-sum(isnull(S_SE_SD_SalesValue,0)) as salesValue
  from S_SE_SalesBasic inner join S_SE_SalesDetail on S_SE_SB_SalesID=S_SE_SD_SalesID
       left join SYS_PersonInfo on S_SE_SB_SalerID=id
       left join B_GoodsInfo on S_SE_SD_SalesItemID=B_GI_GoodsID
  where B_GI_GoodsCategoryID in ('4','5') and B_GI_SupplierID<>'A3' and S_SE_SB_ValueFlag='1' and S_SE_SB_WithdrawFlag='1'
        and S_SE_SB_ShopCode=@shopCode
        and ((isnull(@bgnDate,'')<>'' and convert(varchar(10),S_SE_SB_WithdrawDate,120) >= @bgnDate) or isnull(@bgnDate,'')='')
        and ((isnull(@endDate,'')<>'' and convert(varchar(10),S_SE_SB_WithdrawDate,120) <= @endDate) or isnull(@endDate,'')='')
  group by personName
)temp
  group by salerName
)temp
  group by salerName
)temp
)temp
  order by salesValue desc]]></Query>
</TableData>
