      and B_GI_GoodsCategoryID = '1' 
      and 0 = (select (case count(S_SE_SD_ID) when 0 then 0 else 1 end) from S_SE_SalesDetail where S_SE_SD_SalesID = @salesid and substring(S_SE_SD_SalesItemID,1,1) = '6')
union all
