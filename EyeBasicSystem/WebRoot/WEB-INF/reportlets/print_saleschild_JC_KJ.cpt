<![CDATA[
declare @salesid varchar(50)
set @salesid = '${salesid}'


SELECT [S_SE_SD_SalesItemID]A AS '商品代码'
      ,B_GI_GoodsName AS '商品名称'
      ,[S_SE_SD_SPrice]A AS '商品单价'
      ,[S_SE_SD_Number]A AS '数量'
      ,[S_SE_SD_DiscountRate]A AS '折率'
      ,(S_SE_SD_PriceSum - S_SE_SD_SalesValue) AS '折扣金额'
      ,B_GC_GoodsCategoryName  AS '商品类别'
      ,S_SE_SD_SalesValue AS '应收金额'
      ,B_UT_unitName AS '单位名称'
from  S_SE_SalesDetail inner join S_SE_SalesBasic on S_SE_SD_SalesID = S_SE_SB_SalesID
left join B_GoodsInfo on S_SE_SD_SalesItemID = B_GI_GoodsID left join B_GoodsCategory 
on B_GI_GoodsCategoryID = B_GC_ID left join B_Unit on B_GI_UnitId = B_UT_id
where [S_SE_SD_SalesID]A = @salesid and isnull(S_SE_SD_LargessFlag,'') <> '1' and S_SE_SB_OrdersType in ('1','2')
      and B_GI_GoodsCategoryID = '1' and B_GI_SupplierID <> 'ZZ'
      and 1 = (select (case count(S_SE_SD_ID) when 0 then 0 else 1 end) from S_SE_SalesDetail where S_SE_SD_SalesID = @salesid and substring(S_SE_SD_SalesItemID,1,1) = '6')
union all
SELECT [S_SE_SD_SalesItemID]A AS '商品代码'
      ,B_GI_GoodsName AS '商品名称'
      ,[S_SE_SD_SPrice]A AS '商品单价'
