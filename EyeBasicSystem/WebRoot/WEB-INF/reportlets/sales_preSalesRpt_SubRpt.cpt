<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="startDate"/>
<O>
<![CDATA[2013-07-01]]></O>
</Parameter>
<Parameter>
<Attributes name="shoCode"/>
<O>
<![CDATA[6006]]></O>
</Parameter>
<Parameter>
<Attributes name="branIds"/>
<O>
<![CDATA[3.A1.C4]]></O>
</Parameter>
<Parameter>
<Attributes name="endDate"/>
<O>
<![CDATA[2013-07-23]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[DECLARE @shoCode VARCHAR(1000)
DECLARE @startDate VARCHAR(1000)
DECLARE @endDate VARCHAR(1000)
DECLARE @branIds VARCHAR(100)

SET @shoCode = '${shoCode}'
SET @startDate = '${startDate}'
SET @endDate = '${endDate}'
SET @branIds = '${branIds}'

SELECT 
S_SE_SB_SalesID, 
S_SE_SB_ShopCode, 
B_DP_DepartmentName,
supplierId,
B_SP_SupplierName,
brandId,
B_BD_brandName,
brandIds,
S_SE_SD_SalesItemID, 
S_SE_SD_SalesItemName, 
S_SE_SD_Number, 
S_SE_SD_PriceSum, 
S_SE_SD_SalesValue, 
S_SE_SD_DiscountRate, 
S_SE_SB_PosDatetime, 
S_SE_SB_WithdrawFlag 
FROM (
SELECT     
dbo.S_SE_SalesBasic.S_SE_SB_SalesID, 
dbo.S_SE_SalesBasic.S_SE_SB_ShopCode, 
substring(S_SE_SD_SalesItemID, 3, 2) AS supplierId,
substring(S_SE_SD_SalesItemID, 6, 2) AS brandId,
substring(S_SE_SD_SalesItemID, 1, 7) AS brandIds,
dbo.S_SE_SalesDetail.S_SE_SD_SalesItemID, 
dbo.S_SE_SalesDetail.S_SE_SD_SalesItemName, 
dbo.S_SE_SalesDetail.S_SE_SD_Number, 
dbo.S_SE_SalesDetail.S_SE_SD_PriceSum, 
dbo.S_SE_SalesDetail.S_SE_SD_SalesValue, 
dbo.S_SE_SalesDetail.S_SE_SD_DiscountRate, 
convert(varchar(10), dbo.S_SE_SalesBasic.S_SE_SB_PosDatetime, 23) AS S_SE_SB_PosDatetime, 
dbo.S_SE_SalesBasic.S_SE_SB_WithdrawFlag
FROM         dbo.S_SE_SalesBasic 
	INNER JOIN dbo.S_SE_SalesDetail 
		ON dbo.S_SE_SalesBasic.S_SE_SB_SalesID = dbo.S_SE_SalesDetail.S_SE_SD_SalesID
WHERE isnull(S_SE_SB_PosDatetime,'') <> '' 
	AND S_SE_SB_WithdrawFlag <> '1'

UNion all

SELECT     
dbo.S_SE_SalesBasic.S_SE_SB_SalesID, 
dbo.S_SE_SalesBasic.S_SE_SB_ShopCode, 
substring(S_SE_SD_SalesItemID, 3, 2) AS supplierId,
substring(S_SE_SD_SalesItemID, 6, 2) AS brandId,
substring(S_SE_SD_SalesItemID, 1, 7) AS brandIds,
dbo.S_SE_SalesDetail.S_SE_SD_SalesItemID, 
dbo.S_SE_SalesDetail.S_SE_SD_SalesItemName, 
-dbo.S_SE_SalesDetail.S_SE_SD_Number, 
-dbo.S_SE_SalesDetail.S_SE_SD_PriceSum, 
-dbo.S_SE_SalesDetail.S_SE_SD_SalesValue, 
dbo.S_SE_SalesDetail.S_SE_SD_DiscountRate, 
dbo.S_SE_SalesBasic.S_SE_SB_PosDatetime, 
dbo.S_SE_SalesBasic.S_SE_SB_WithdrawFlag
FROM         dbo.S_SE_SalesBasic 
	INNER JOIN dbo.S_SE_SalesDetail 
		ON dbo.S_SE_SalesBasic.S_SE_SB_SalesID = dbo.S_SE_SalesDetail.S_SE_SD_SalesID
WHERE isnull(S_SE_SB_PosDatetime,'') <> '' 
	AND S_SE_SB_WithdrawFlag = '1'
) AS t1
	INNER JOIN B_Departments
		ON B_DP_DepartmentID = S_SE_SB_ShopCode
	INNER JOIN B_Supplier 
		ON B_SP_ID = supplierId
	INNER JOIN B_Brand
		ON B_BD_ID = brandId AND B_BD_SupplierID = supplierId
WHERE
	S_SE_SB_ShopCode = @shoCode
	AND S_SE_SB_PosDatetime >= @startDate
	AND S_SE_SB_PosDatetime <= @endDate
	AND brandIds = @branIds]]></Query>
</TableData>
</TableDataMap>
