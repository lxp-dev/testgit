<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[select stockId into #stroageStockId from storage_createStockIdTab('${stock}')
select *
from(select tt.GoodsCategoryID as GoodsCategoryID,tt.GoodsCategoryName as GoodsCategoryName,
tt.SupplierID as SupplierID,tt.SupplierName as SupplierName,tt.brandName as brandName,
tt.brandID as brandID,tt.StockId as StockId ,tt.warehouseName as warehouseName,tt.DepartmentsType as DepartmentsType,tt.GoodsId as GoodsId,tt.GoodsName as GoodsName,tt.RetailPrice as RetailPrice,
sum(tt.GoodsQuantity) as GoodsQuantity,sum(tt.CostPrice) as CostPrice
from(select t.GoodsId as GoodsId,B_GI_GoodsName as GoodsName,B_WH_warehouseName as warehouseName,B_DP_Type as DepartmentsType,
B_GI_SupplierID as SupplierID,B_SP_SupplierName as SupplierName,B_BD_brandName as brandName,
B_GC_ID as GoodsCategoryID,B_GC_GoodsCategoryName as GoodsCategoryName,
B_BD_ID as brandID,t.StockId as StockId, sum(t.GoodsQuantity) as GoodsQuantity,sum(t.GoodsQuantity*B_GI_CostPrice) as CostPrice,B_GI_RetailPrice as RetailPrice
 from 
(select C_SH_SB_GoodsId as GoodsId,C_SH_SB_StockId as StockId,C_SH_SB_GoodsQuantity as GoodsQuantity
 from C_SH_StorageBeginning
union all 
select C_SH_SC_GoodsId as GoodsId,C_SH_SC_StockId as StockId,C_SH_SC_GoodsQuantity as GoodsQuantity
from C_SH_StorageChange)t
inner join B_GoodsInfo on t.GoodsId=B_GI_GoodsID
inner join B_GoodsCategory on B_GI_GoodsCategoryID=B_GC_ID
inner join B_Supplier on B_SP_ID=B_GI_SupplierID
inner join B_Brand on B_BD_SupplierID=B_GI_SupplierID  and B_BD_ID=B_GI_BrandID
inner join B_Warehouse on t.StockId=B_WH_ID
inner join B_Departments on B_DP_DepartmentID = B_WH_deptID
where 1=1 
and ((isnull('${goodsCategory}','')<>'' and B_GI_GoodsCategoryID='${goodsCategory}' ) or (isnull('${goodsCategory}','')=''))
and ((isnull('${stock}','')<>'' and t.StockId in (select StockId from #stroageStockId)) or (isnull('${stock}','')=''))
and ((isnull('${brandID}','')<>'' and B_BD_ID='${brandID}') or (isnull('${brandID}','')=''))
and ((isnull('${goodsID}','')<>'' and B_BD_ID='${goodsID}') or (isnull('${goodsID}','')=''))
and ((isnull('${supplier}','')<>'' and B_SP_ID='${supplier}') or (isnull('${supplier}','')=''))
group by t.GoodsId,B_GI_GoodsName,B_GI_RetailPrice,B_WH_warehouseName,B_GI_SupplierID,B_SP_SupplierName,B_BD_brandName,
B_GC_ID,B_GC_GoodsCategoryName,B_BD_ID,t.StockId,B_DP_Type)tt
group by tt.GoodsCategoryID,tt.DepartmentsType,tt.GoodsCategoryName,tt.SupplierID,tt.SupplierName,tt.brandID,tt.brandName,tt.warehouseName, tt.StockId,tt.GoodsId,tt.GoodsName,tt.RetailPrice
)ttt 
where ttt.GoodsQuantity>0
order by ttt.DepartmentsType desc,ttt.StockId desc,ttt.GoodsCategoryName,ttt.SupplierName,ttt.brandName
drop table #stroageStockId]]></Query>
</TableData>
