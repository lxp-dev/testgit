<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource1]]></DatabaseName>
</Connection>
<Query>
<![CDATA[DECLARE @begintime  VARCHAR(50),
        @endtime  VARCHAR(50),
        @departmentid VARCHAR(50)

SET @begintime='${begintime}'
SET @endtime='${endtime}'
SET @departmentid='${departmentid}'

select b.PInfo_ID,per.P_Code,per.P_Name,dept.Dept_Name, Datepart(weekday,b.dy) as color, b.dy,right(Date_Time,8) as dtime from (select PInfo_ID,Date_Time,convert(varchar(10),Date_Time,120)  AS dy from Record_TA  ) as b  left join Ren_Per_Info per on b.PInfo_ID=per.PInfo_id left join Ren_Dept_Info dept on dept.Dept_ID=per.Dept_ID  where  convert(varchar(10),b.dy,23) >= @begintime 
     and convert(varchar(10),b.dy,23) <= @endtime  
and per.Dept_ID in (select dinfo.Dept_ID from Ren_Dept_Info dinfo  where 
 case when @departmentid =''  then @departmentid else dinfo.Dept_Code end  = @departmentid )
order by b.PInfo_ID,b.dy, b.Date_Time  ]]></Query>
</TableData>
<TableData name="ds2" class="com.fr.data.impl.DBTableData">
<Parameters/>
<Attributes maxMemRowCount="-1"/>
