<TableDataMap>
<TableData name="ds1" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="salesid"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[

select
S_SE_SD_SalesItemID AS '商品代码',
case 
when S_SE_SD_CommoditiesFlag = '8' then B_GI_GoodsName + '  ' +B_GI_Sph 
 WHEN (S_SE_SD_CommoditiesFlag='4' OR S_SE_SD_CommoditiesFlag='5') AND F_SP_PrintStealthCheck='1' THEN (S_SE_SD_SalesItemName+'：'+ISNULL(S_SE_SD_Guaranteeperiod,'') )
else B_GI_GoodsName end as '商品名称',
case 
when S_SE_SD_CommoditiesFlag = '2' then '99'
else S_SE_SD_CommoditiesFlag end as '排序',
case
when S_SE_SD_CommoditiesFlag = '1' then B_GI_Spec + '-' + B_GI_Color+ '-' + B_GI_FrameSize
when S_SE_SD_CommoditiesFlag = '6' then B_GI_Spec + '-' + B_GI_Color+ '-' + B_GI_FrameSize
else B_GI_Spec end as '商品规格',
B_UT_unitName AS '单位名称',
S_SE_SD_Number AS '数量',
S_SE_SD_PriceSum AS '金额合计',
S_SE_SD_DiscountRate AS '商品折扣'
from F_SystemParameter,
S_SE_SalesDetail inner join S_SE_SalesBasic on S_SE_SB_SalesID=S_SE_SD_SalesID
left join B_GoodsInfo on B_GI_GoodsID = S_SE_SD_SalesItemID
left join B_Unit on B_GI_UnitId = B_UT_id
where S_SE_SD_SalesID = '${salesid}'

union all 


select
S_SE_SD_SalesItemID AS '商品代码',
case 
when S_SE_SD_CommoditiesFlag = '8' then B_GI_GoodsName + '  ' +B_GI_Sph 
 WHEN (S_SE_SD_CommoditiesFlag='4' OR S_SE_SD_CommoditiesFlag='5') AND F_SP_PrintStealthCheck='1' THEN (S_SE_SD_SalesItemName+'：'+ISNULL(S_SE_SD_Guaranteeperiod,'') )
else B_GI_GoodsName end as '商品名称',
case 
when S_SE_SD_CommoditiesFlag = '2' then '99'
else S_SE_SD_CommoditiesFlag end as '排序',
case
when S_SE_SD_CommoditiesFlag = '1' then B_GI_Spec + '-' + B_GI_Color+ '-' + B_GI_FrameSize
when S_SE_SD_CommoditiesFlag = '6' then B_GI_Spec + '-' + B_GI_Color+ '-' + B_GI_FrameSize
else B_GI_Spec end as '商品规格',
B_UT_unitName AS '单位名称',
S_SE_SD_Number AS '数量',
S_SE_SD_PriceSum AS '金额合计',
S_SE_SD_DiscountRate AS '商品折扣'
from F_SystemParameter,
S_SE_SalesDetail_Finished inner join S_SE_SalesBasic_Finished on S_SE_SB_SalesID=S_SE_SD_SalesID
left join B_GoodsInfo on B_GI_GoodsID = S_SE_SD_SalesItemID
left join B_Unit on B_GI_UnitId = B_UT_id
where S_SE_SD_SalesID = '${salesid}'
ORDER BY [排序]A]]></Query>
</TableData>
<TableData name="商品信息" class="com.fr.data.impl.DBTableData">
<Parameters>
<Parameter>
<Attributes name="salesID"/>
<O>
<![CDATA[]]></O>
</Parameter>
</Parameters>
<Attributes maxMemRowCount="-1"/>
<Connection class="com.fr.data.impl.NameDatabaseConnection">
<DatabaseName>
<![CDATA[datasource]]></DatabaseName>
</Connection>
<Query>
<![CDATA[SELECT TOP 7
--------关联商品表,取规格,工艺类型 begin--------------
CASE
  WHEN B_GI_GoodsCategoryID = '1' THEN Isnull(B_GI_GoodsName, '') +  Isnull('—' + Rtrim(B_GI_Color), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN B_GI_GoodsCategoryID = '6' THEN Isnull(B_GI_GoodsName, '') +  Isnull('—' + Rtrim(B_GI_Color), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN B_GI_GoodsCategoryID = '8' THEN Isnull(B_GI_GoodsName, '') +  Isnull('—' + Rtrim(B_GI_SupplierColor), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN ( B_GI_GoodsCategoryID = '4'
          OR B_GI_GoodsCategoryID = '5' )
       AND F_SP_PrintStealthCheck = '1' THEN ( B_GI_GoodsName + '：' + Isnull(S_SE_SD_Guaranteeperiod, '') )
  ELSE Isnull(B_GI_GoodsName, '')
END                                         AS '商品名称',
--------关联商品表,取规格,工艺类型 end--------------
Isnull(S_SE_SD_Number, 0)   AS '商品数量',
Isnull(S_SE_SD_SPrice, 0)                   AS '商品单价',
Isnull(S_SE_SD_PriceSum, 0) AS '商品原价',
Isnull(S_SE_SD_SalesValue, 0)               AS '商品应收金额',
Isnull(S_SE_SB_SalesValue, 0)               AS '商品总计金额',
Isnull(B_GI_UnitId, 0)                      AS '计量单位',
S_SE_SD_DiscountRate                        AS '折扣率',
B_UT_unitName                               AS '计量单位名称',
B_GI_Spec                                   AS '商品规格'
FROM   F_SystemParameter,
       S_SE_SalesBasic
       INNER JOIN S_SE_SalesDetail
         ON S_SE_SB_SalesID = S_SE_SD_SalesID
       -------关联商品表,取规格,工艺类型 add by zk begin------------
       LEFT JOIN B_GoodsInfo
         ON B_GI_GoodsID = S_SE_SalesDetail.S_SE_SD_SalesItemID
left join B_Unit on B_UT_id=B_GI_UnitId
       LEFT JOIN F_TechnologyType
         ON B_GoodsInfo.B_GI_frameProcessCraftType = F_TechnologyType.F_TT_ID
-------关联商品表,取规格,工艺类型 add by zk enb------------
WHERE  S_SE_SB_SalesID = '${salesID}'
       AND S_SE_SB_ValueFlag = '1'
       AND Isnull(S_SE_SD_LargessFlag, '0') = '0'
union all 
SELECT TOP 7
--------关联商品表,取规格,工艺类型 begin--------------
CASE
  WHEN B_GI_GoodsCategoryID = '1' THEN Isnull(B_GI_GoodsName, '') +  Isnull('—' + Rtrim(B_GI_Color), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN B_GI_GoodsCategoryID = '6' THEN Isnull(B_GI_GoodsName, '') +  Isnull('—' + Rtrim(B_GI_Color), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN B_GI_GoodsCategoryID = '8' THEN Isnull(B_GI_GoodsName, '') +  Isnull('—' + Rtrim(B_GI_SupplierColor), '') + Isnull('—' + Rtrim(F_TT_Name), '')
  WHEN ( B_GI_GoodsCategoryID = '4'
          OR B_GI_GoodsCategoryID = '5' )
       AND F_SP_PrintStealthCheck = '1' THEN ( B_GI_GoodsName + '：' + Isnull(S_SE_SD_Guaranteeperiod, '') )
  ELSE Isnull(B_GI_GoodsName, '')
END                                         AS '商品名称',
--------关联商品表,取规格,工艺类型 end--------------
Isnull(S_SE_SD_Number, 0)   AS '商品数量',
Isnull(S_SE_SD_SPrice, 0)                   AS '商品单价',
Isnull(S_SE_SD_PriceSum, 0) AS '商品原价',
Isnull(S_SE_SD_SalesValue, 0)               AS '商品应收金额',
Isnull(S_SE_SB_SalesValue, 0)               AS '商品总计金额',
Isnull(B_GI_UnitId, 0)                      AS '计量单位',
S_SE_SD_DiscountRate                        AS '折扣率',
B_UT_unitName                               AS '计量单位名称',
B_GI_Spec                                   AS '商品规格'
FROM   F_SystemParameter,
       S_SE_SalesBasic_Finished
       INNER JOIN S_SE_SalesDetail_Finished
         ON S_SE_SB_SalesID = S_SE_SD_SalesID
       -------关联商品表,取规格,工艺类型 add by zk begin------------
       LEFT JOIN B_GoodsInfo
         ON B_GI_GoodsID = S_SE_SalesDetail_Finished.S_SE_SD_SalesItemID
left join B_Unit on B_UT_id=B_GI_UnitId
       LEFT JOIN F_TechnologyType
         ON B_GoodsInfo.B_GI_frameProcessCraftType = F_TechnologyType.F_TT_ID
-------关联商品表,取规格,工艺类型 add by zk enb------------
WHERE  S_SE_SB_SalesID = '${salesID}'
       AND S_SE_SB_ValueFlag = '1'
       AND Isnull(S_SE_SD_LargessFlag, '0') = '0']]></Query>
</TableData>
