<script>	
	function search(){
		if(checkForm(goodsForm)){
			var isShow = "";
			$("input[id=isShow]").each(function() {
				if($(this).attr("checked") == true) {
					isShow = $(this).val();
				}
			});
			var DataURL = "report.action?reportlet=lead_ManageStream.cpt&__bypagesize__=false&queryYear="+$('#year').val()+"&queryMonth="+$('#month').val()+'&isShow='+isShow;
			var topRows = top.document.getElementById("total").rows;
			var topCols = top.document.getElementById("btmframe").cols;		
			if(is_iPad()){
				showPopWin(DataURL,970,screen.height-200,topRows,topCols,returnRefresh(true),false);
			}else{
				openWindowForReport(DataURL); 
			}		
			document.getElementById('popupTitle').innerHTML="【经营流水统计表】";
		}
	}
	function clean(){
		goodsForm.reset();
		document.getElementById('year').value = "";
		document.getElementById('month').value = "";
	}

	$(document).ready(function() { 
	    $("img[btn=btn]").attr("style","cursor: hand;"); 
	    $("img[btn=btn]").mouseover(function () { 
	    $(this).attr("src",$(this).attr("src").replace("0","1")); 
	    }).mouseout(function () { 
	      $(this).attr("src",$(this).attr("src").replace("1","0")); 
	    }); 
    }); 

</script>
