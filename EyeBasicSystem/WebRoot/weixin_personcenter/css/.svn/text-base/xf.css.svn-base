.item-checkbox .checkbox {
	position: absolute;
	position: absolute;
	top: 74%;
	right: 19px;
	left: 20%;
	z-index: 3;
	margin-top: -4.6%;
}
.checkbox {
	display: inline-block;
	cursor: pointer;
}

.checkbox input, .checkbox-icon {
	position: relative;
	display: block;
	border: 0;
	background: 0 0;
	cursor: pointer;  top: 3.2px;
	-webkit-appearance: none;
}

.checkbox input:checked:before, input:checked+.checkbox-icon:before {
	border-width: 1px;
}
.checkbox input:checked+.checkbox-icon:before, .checkbox input:checked:before {
	background: #eeeeee;
	border-color: #bfbfbf;
}
.checkbox input:before, .checkbox-icon:before {
	display: table;
	width: 1rem;
	height: 1rem;
	border-width: 1px;
	border-style: solid;
	background: #ecebea;
	content: ' ';
	-webkit-transition: background-color 20ms ease-in-out;
	transition: background-color 20ms ease-in-out;
	border:1px solid #e4e4e3;
}
.checkbox input:after, .checkbox-icon:after {
	-webkit-transition: opacity .05s ease-in-out;
	transition: opacity .05s ease-in-out;
	-webkit-transform: rotate(-45deg);
	transform: rotate(-45deg);
	position: absolute;
	top: 4%;
	left: 2%;
	display: table;
	width: 14px;
	height: 6px;
	border: 2px solid #70d1c3;
	border-top: 0;
	border-right: 0;
	content: ' ';
	opacity: 0;
}
.checkbox input:checked:after, input:checked+.checkbox-icon:after {
	opacity: 1;
}
